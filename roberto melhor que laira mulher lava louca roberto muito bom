                                        firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v, 0)
                                        wait()
                                    end
                                end
                            end)
                            SupportGame:CreateHeart('Auto Farm (All)', function()
                                for i,v in pairs(workspace:GetDescendants()) do
                                    if v.Name == 'outerOrb' or v.Name == 'outerGem' or v.Name == 'Hoop' then
                                        firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v, 0)
                                    end
