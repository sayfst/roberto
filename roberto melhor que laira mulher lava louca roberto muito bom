                                noclipSupport(b)
                            end)

                            SupportGame:CreateTextLabel('Esp')


                            local espColor = Color3.fromRGB(0, 255, 0)
                            local MaxDistance = 0
                            local Isle_Esp = false

                            SupportGame:CreateTextBox('Esp Color (RGB)', 'String', function(color)
                                local p = color:split(', ')
                                local color = Color3.fromRGB(p[1], p[2], p[3])

                                espColor = color
                            end)

                            SupportGame:CreateSlider('Max distance Esp', 5, 500, function(b)
                                MaxDistance = b
                            end)
                            local Esps = {}
                            SupportGame:CreateToggle('Esp', function(bool, force_active, is_forced)
                                Isle_Esp = bool

                                if bool then
                                    for i,v in pairs(game:GetService("Workspace").Tools:GetChildren()) do
                                        if v.ClassName == 'Model' and v.Handle then
                                            local Line = Drawing.new('Line')
                                            Line.Thickness = 0.3 
                                            Line.Visible = false
                                            Line.Thickness = 0.3
                                            Line.Color = espColor
                                            local Handle = v.Handle
                                            local camera = workspace.CurrentCamera
                                            task.spawn(function()
                                                repeat
                                                    wait()
                                                until Isle_Esp == false
                                                task.spawn(function()
                                                    Line:Remove()
                                                end)
                                            end)
                                            function track()
                                                game:GetService('RunService').RenderStepped:Connect(function()
                                                    local Vector, OnScreen = camera:worldToViewportPoint(Handle.Position)
                                                    
                                                    if OnScreen and (getRoot(speaker).Position - Handle.Position).Magnitude < MaxDistance then
                                                        Line.Visible = true
                                                        Line.From = Vector2.new(camera.ViewportSize.X / 2, camera.ViewportSize.Y / 1)
                                                        Line.To = Vector2.new(Vector.x, Vector.y)
                                                        Line.Color = espColor
                                                    else
                                                        Line.Visible = false
                                                    end
                                                end)
                                            end

                                            coroutine.wrap(track)()
                                        end
                                    end
                                end
                            end)
                            SupportGame:CreateTextLabel('Ray X')


                            ray_x = false

                            SupportGame:CreateButton('Ray X', function()
                                ray_x = true
                                for i,v in pairs(workspace:GetDescendants()) do
                                    if v:IsA('BasePart') and v.Transparency ~= 1 then
                                        task.spawn(function()
                                            local oldTransparency = v.Transparency
                                            local ray = game.RunService.Stepped:Connect(function()
                                                v.Transparency = 0.5
                                            end)
                                            task.spawn(function()
                                                repeat wait() until ray_x == false
                                                ray:Disconnect()
                                                v.Transparency = oldTransparency
                                            end)
                                        end)
                                    end
                                end
                            end)
                            SupportGame:CreateButton('UnRay X', function()
                                ray_x = false
                            end)

                        end
                        --[[Snowman Simulator]] if game.PlaceId == 2533391464 then
                            ChangeName('Christmas Simulator')

                            SupportGame:CreateTextLabel('Auto Farm')


                            local function FindBase(speaker)
                                for i,v in pairs(game:GetService("Workspace").snowmanBases:GetChildren()) do
                                    if v.Name == 'LandPlot' then 
                                        if v:FindFirstChild('player').Value == speaker then
                                            return v
                                        end
                                    end
                                end
                            end
                            local function AddToSnowMan()
                                local args = {
                                    [1] = "addToSnowman"
                                }
                                
                                game:GetService("ReplicatedStorage").Signals.addToSnowman:FireServer(unpack(args))                                
                            end
                            local function ResetSnowMan()
                                local ClickDetector = FindBase(speaker):FindFirstChild('rebirthControl'):FindFirstChild('BUTTON'):FindFirstChild('ClickDetector')

                                fireclickdetector(ClickDetector)
                            end
                            local function CheckIsFullInv()
                                local split = game:GetService("Players")[game.Players.LocalPlayer.Name].PlayerGui.Main.hudItems.snowballs.textFrame.TextLabel.Text:split('/')
                                
                                if split[1] == split[2] then
                                    return true
                                end
                                return false
                            end
                            local function GetTool()
                                return getCharacter(speaker):FindFirstChild('snowmanBall')
                            end
                            local function CheckSnowFull()
                                if FindBase(speaker).progressPosition.progressBar.background.bar.text.Text == 'MAX' then
                                    return true
                                end
                                return false
                            end

                            local canAim = false
                            
                            function startt()
                                SnowFarm = true
                                task.spawn(function()
                                    task.spawn(function()
                                        while SnowFarm == true do
                                            speaker.Character.HumanoidRootPart.CFrame = FindBase(speaker):FindFirstChild('rebirthControl'):FindFirstChild('BUTTON').CFrame * CFrame.new(0, 2.5, 0)
                                            local posToWalk = getRoot(speaker).CFrame * CFrame.new(0, 0, -5)
                                            speaker.Character.Humanoid:MoveTo(posToWalk.Position)
                                            if CheckIsFullInv() and _G.autosell == false then
                                                StartError()
                                            else
                                                StopError()
                                            end
                                            task.wait()    
                                        end
                                    end)
                                    task.spawn(function()
                                        while SnowFarm == true do
                                            task.spawn(function()
                                                if not getCharacter(speaker):FindFirstChild('snowmanBall') then
                                                    keypress(keycodeMap['r'])
                                                    wait()
                                                    keyrelease(keycodeMap['r'])
                                                    
                                                end
                                            end)
                                            task.wait()
                                        end
                                    end)
                                    while SnowFarm == true do 
                                        
                                        
                                        for i,v in pairs(game:GetService("Workspace").gameSnow:GetChildren()) do
                                            task.spawn(function()
                                                if canAim then workspace.CurrentCamera.CFrame = CFrame.new(workspace.CurrentCamera.CFrame.Position, v.Position) end
                                                firetouchinterest(GetTool(), v, 0)
                                                wait(0.01)
                                                firetouchinterest(GetTool(), v, 1)
                                            end)
                                            
                                            wait()
                                        end
                                        wait()
                                    end
                                end)
                            end
                            function stopp()
                                SnowFarm = false
                            end

                            SupportGame:CreateToggle('Auto farm', function(b)
                                if b then
                                    startt()
                                else
                                    stopp()
                                end
                            end)
                            SupportGame:CreateToggle('Aim to Snow select', function(b) canAim = b end)
                            SupportGame:CreateToggle('Auto Sell and Auto Reset snowman', function(b)
                                _G.autosell = b 
                                if b then
                                    while _G.autosell == true do
                                        if CheckIsFullInv() then
                                            StopError()
                                            AddToSnowMan()
                                        end
                                        if CheckSnowFull() then
                                            ResetSnowMan()
                                        end
                                        wait(1)
                                    end
                                end
                            end)
                            local hits=50
                            SupportGame:CreateSlider('Kill aura boss (Hits)', 10, 500, function(b)
                                hits = b
                            end)
                            SupportGame:CreateButton('Kill Aura Boss', function()
                                for i,v in pairs(game:GetService("Workspace").steps:GetDescendants()) do
                                    if v.Name == 'Boss' and v.ClassName == 'Model' then
                                        if (getRoot(speaker).Position - v.Head.Position).Magnitude <=75 then
                                            for i = 1,hits do
                                                local args = {
                                                    [1] = "explodeLauncher",
                                                    [2] = v.Head.Position
                                                }
    
                                                game:GetService("ReplicatedStorage").Signals.snowballProjectile:FireServer(unpack(args))
    
                                            end
                                        end

                                    end
                                end
                            end)
                            SupportGame:CreateButton('Kill all boss', function()
                                for i,v in pairs(game:GetService("Workspace").steps:GetDescendants()) do
                                    if v.Name == 'Boss' and v.ClassName == 'Model' then
                                        if v.Head then

                                            task.spawn(function()
                                                for i = 1,hits do
                                                    local args = {
                                                        [1] = "explodeLauncher",
                                                        [2] = v.Head.Position
                                                    }
        
                                                    game:GetService("ReplicatedStorage").Signals.snowballProjectile:FireServer(unpack(args))
        
                                                end
                                            end)
                                        end

                                    end
                                end
                            end)
                            
                            SupportGame:CreateToggle('Auto kill Mobs', function(b)
                                _G.isfarm = b 

                                if b then
                                    while _G.isfarm == true do
                                        for i,v in pairs(game:GetService("Workspace").minionSpawns:GetDescendants()) do
                                            if v.Name == 'hitBox' and _G.isfarm == true then
                                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
                                                wait(.1)
                                                local args = {
                                                    [1] = "explodeLauncher",
                                                    [2] = v.Position
                                                }
                                                
                                                game:GetService("ReplicatedStorage").Signals.snowballProjectile:FireServer(unpack(args))
                                                oldpos = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
                                                wait(.3)
                                                for i,v in pairs(game:GetService("Workspace").gameCandyCanes:GetDescendants()) do
                                                    if v.Name == 'cane' then
                                                        if (v.Position - oldpos).Magnitude <=20 then
                                                            for i = 1,10 do
                                                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
                                                                wait()
                                                            end
                                                        end
                                                    end
                                                end
                                                wait(.2)
                                            end
                                        end
                                        task.wait()
                                    end
                                end
                            end)

                            SupportGame:CreateTextLabel('Auto Open gifs')

                            SupportGame:CreateButton('Open Random Gift', function()
                                oldpos = getRoot(speaker).CFrame
                                for i,v in pairs(game:GetService("Workspace").giftSpawns:GetChildren()) do
                                    getRoot(speaker).CFrame = v.hitbox.CFrame + Vector3.new(0, 2, 0)
                                    wait(0.2)
                                    workspace.CurrentCamera.CFrame = CFrame.new(workspace.CurrentCamera.CFrame.Position, v.hitbox.Position)
                                    wait(0.3)
                                    fireproximityprompt(v.hitbox.proxGui.ProximityPrompt)
                                    for i = 1, 1000 do
                                        if v.hitbox then
                                            getRoot(speaker).CFrame = v.hitbox.CFrame
                                            task.wait()    
                                        end
                                    end
                                    getRoot(speaker).CFrame = oldpos
                                    break;
                                end
                            end)
                            SupportGame:CreateButton('Open all gifs', function()
                                oldpos = getRoot(speaker).CFrame
                                for i,v in pairs(game:GetService("Workspace").giftSpawns:GetChildren()) do
                                    getRoot(speaker).CFrame = v.hitbox.CFrame + Vector3.new(0, 2, 0)
                                    wait(0.2)
                                    workspace.CurrentCamera.CFrame = CFrame.new(workspace.CurrentCamera.CFrame.Position, v.hitbox.Position)
                                    wait(0.3)
                                    fireproximityprompt(v.hitbox.proxGui.ProximityPrompt)
                                    for i = 1, 1000 do
                                        if v.hitbox then
                                            getRoot(speaker).CFrame = v.hitbox.CFrame
                                            task.wait()    
                                        end
                                    end
                                    getRoot(speaker).CFrame = oldpos
                                end
                            end)

