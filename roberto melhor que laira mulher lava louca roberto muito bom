                                                repeat
                                                    timeUsing = timeUsing + 0.05
                                                    getRoot(speaker).CFrame = v.HumanoidRootPart.CFrame + Vector3.new(0, 5, 8)
                                                    task.wait()
                                                    sell(v)
                                                until v:FindFirstChild('HasIceCream') == nil or v:FindFirstChild('HumanoidRootPart') == nil or v:FindFirstChild('HasIceCream').Value == true or timeUsing > 1 
                                            end
                                        end
                                    end
                                end
                                repeat
                                    getRoot(speaker).CFrame = oldpos
                                    wait(0.1)
                                until (getRoot(speaker).Position - oldpos.Position).Magnitude < 10
                            end)
                            b:CreateTextLabel('Items')

