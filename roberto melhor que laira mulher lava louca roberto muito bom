                                    force('toggled')
                                end)
                            end
                            function botoes:CreateDropDown(title, tab, callback)
                                tab = tab or {}
                                local DropDown = Instance.new("TextLabel")
                                local ScrollingFramee = Instance.new("ScrollingFrame")    
                                local UIListLayout = Instance.new("UIListLayout")
                                local UICorner = Instance.new("UICorner")
                                local add_circle_outline = Instance.new("ImageButton")
                                
                                
                                
                                DropDown.Name = "DropDown"
                                DropDown.Parent = ScrollingFrame
                                DropDown.Active = true
                                DropDown.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                                DropDown.BackgroundTransparency = 1.000
                                DropDown.Position = UDim2.new(0, 0, 0.248434171, 0)
                                DropDown.Selectable = true
                                DropDown.Size = UDim2.new(1, 0, 0, 18)
                                DropDown.Font = Enum.Font.RobotoCondensed
                                DropDown.Text = title
                                DropDown.TextColor3 = Color3.fromRGB(255, 255, 255)
                                DropDown.TextSize = 14.000
                                DropDown.TextXAlignment = Enum.TextXAlignment.Left
                                DropDown.TextYAlignment = Enum.TextYAlignment.Top
                                
                                ScrollingFramee.Parent = DropDown
                                ScrollingFramee.Active = true
                                ScrollingFramee.BackgroundColor3 = Color3.fromRGB(36, 36, 36)
                                ScrollingFramee.BackgroundTransparency = 1.000
                                ScrollingFramee.BorderSizePixel = 0
                                ScrollingFramee.Position = UDim2.new(0, 0, 0.314285725, 0)
                                ScrollingFramee.Size = UDim2.new(1, 0, 0.685714304, 0)
                                ScrollingFramee.Visible = false
                                ScrollingFramee.ScrollBarThickness = 15
                                ScrollingFramee.AutomaticCanvasSize = Enum.AutomaticSize.Y
                                
                                
                                UIListLayout.Parent = ScrollingFramee
                                UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
                                UIListLayout.Padding = UDim.new(0, 3)   


                                UICorner.CornerRadius = UDim.new(0, 3)
                                UICorner.Parent = Botao
                                
                                add_circle_outline.Name = "add_circle_outline"
                                add_circle_outline.Parent = DropDown
                                add_circle_outline.AnchorPoint = Vector2.new(0.5, 0.5)
                                add_circle_outline.BackgroundTransparency = 1.000
                                add_circle_outline.Position = UDim2.new(0.594444406, 0, 0.142857149, 0)
                                add_circle_outline.Size = UDim2.new(0, 20, 0, 20)
                                add_circle_outline.ZIndex = 2
                                add_circle_outline.Image = "rbxassetid://3926307971"
                                add_circle_outline.ImageRectOffset = Vector2.new(764, 164)
                                add_circle_outline.ImageRectSize = Vector2.new(36, 36)

                                local isopened = false
                                local canopened = true
                                
                                local function toggle()
                                    if canopened then
                                        canopened = false
                                        if isopened == true then
                                            isopened = false
                                            DropDown.Size = UDim2.new(1, 0, 0, 18)
                                            ScrollingFramee.Visible = false
                                        else
                                            isopened = true
                                            DropDown.Size = UDim2.new(1, 0, 0, 120)
                                            ScrollingFramee.Visible = true
                                        end
                                        canopened = true
                                    end
                                end
                                add_circle_outline.MouseButton1Click:Connect(function()
                                    toggle()
                                end)
                                function newbutton(title,volta)
                                    local Botao = Instance.new("TextButton")

                                    Botao.Name = "Botao"
                                    Botao.Parent = ScrollingFramee
                                    Botao.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
                                    Botao.BorderSizePixel = 0
                                    Botao.Size = UDim2.new(0, 180, 0, 14)
                                    Botao.Font = Enum.Font.RobotoCondensed
                                    Botao.Text = title
                                    Botao.TextColor3 = Color3.fromRGB(255, 255, 255)
                                    Botao.TextSize = 14.000
                                    Botao.MouseButton1Click:Connect(function()
                                        toggle()
                                        pcall(callback, volta)
                                    end)
                                end


                                for i,v in pairs(tab) do
                                    newbutton(v[1], v[2])
                                end
                            end
                            return botoes
                            
                        end
                        return newtab
                        
                    end
                    getgenv().ExecCmd = {}
                    getgenv().Toggle = {}
                    function ExecCmd(cmd)
                        for i,v in pairs(getgenv().ExecCmd) do
                            if v.tt == cmd then
                                pcall(function()
                                    v.ff()
                                end)
                            end
                        end
                    end
                    function ToggleToggle(cmd)
                        for i,v in pairs(getgenv().Toggle) do
                            if v.tt == cmd then
                                pcall(function()
                                    v.ff()
                                end)
                            end
                        end
                    end
                    local speaker = game.Players.LocalPlayer
                    local exploit = 'unknown exploit'
                    pcall(function() if syn then exploit = 'Synapse' end end)
                    pcall(function() if KRNL_LOADED then exploit = 'Krnl' end end)
                    pcall(function() if PROTOSMASHER_LOADED then exploit = 'Protosmasher' end end)
                    pcall(function() if issentinelclosure then exploit = 'Sentinel' end end)
                    pcall(function() if is_sirhurt_closure then exploit = 'Sirhurt' end end)
                    pcall(function() if IS_COCO_LOADED then exploit = 'Coco' end end)
                    pcall(function() if isvm then exploit = 'Proxo' end end)
                    pcall(function() if shadow_env  then exploit = 'Shadow' end end)
                    pcall(function() if IS_VIVA_LOADED   then exploit = 'Viva' end end)
                    pcall(function() if WrapGlobal    then exploit = 'Wearedevs' end end)
                    local theme
                    if exploit == 'Synapse' then
                        theme = 'rbxassetid://9818810011'
                    elseif exploit == 'Krnl' then
                        theme = 'rbxassetid://9288455318'
                    end
                    local Menu = magnata:CreateWindow('RICARDO MENU', {
                        ['Theme'] = theme
                    })

                    local rr = {

                    }
                    function rainbow(item, duration, nome)
                        local color = item
                        local tempoRGB =  duration
                        rr[#rr + 1] = nome
                        task.spawn(function()
                            while true do
                                if table.find(rr, nome) then
                                    for i = 1,255 do
                                        color = Color3.fromRGB(255, i, 0)
                                        task.wait(tempoRGB)
                                    end
                                    for i = 255,0, -1 do
                                        color = Color3.fromRGB(i, 255, 0)
                                        task.wait(tempoRGB)
                                    end
                                    for i = 1, 255 do
                                        color = Color3.fromRGB(0, 255, i)
                                        task.wait(tempoRGB)
                                    end
                                    for i = 255,0, -1 do
                                        color = Color3.fromRGB(0, i, 255)
                                        task.wait(tempoRGB)
                                    end
                                    for i = 1, 255 do
                                        color = Color3.fromRGB(i, 0, 255)
                                        task.wait(tempoRGB)
                                    end
                                    for i = 255,0, -1 do
                                        color = Color3.fromRGB(255, 0, i)
                                        task.wait(tempoRGB)
                                    end
                                end
                                task.wait()
                            end
                        end)                       
                    end
                    function unrainbow(item)
                        for i = 1,#rr do
                            if rr[i] == item then
                                table.remove(rr, i)
                            end
                        end
                    end

                    local TabEsp = Menu:CreateTab('Esp')
                    
                    function R15(speaker)
                        if not getHumanoid(speaker) then
                            return false
                        end
                        if speaker.Character.Humanoid.RigType == Enum.HumanoidRigType.R15 then
                            return true;
                        else
                            return false;
                        end
                    end
                    function FindAccessory(name)
                        if typeof(name) == 'string' then
                            if speaker.Character:FindFirstChild(name) then
                                return true;
                            end
                            return false
                        elseif typeof(name) == 'table' then
                            local On = false
                            for i,v in pairs(name) do
                                if speaker.Character:FindFirstChild(v) then
                                    On = true
                                else
                                    On = false 
                                end
                                
                            end
                            return On
                        end
                    end
                    function getCharacter(plr)
                        if plr.Character ~= nil then
                            return plr.Character
                        else
                            return false
                        end
                    end
                    function checkCharacter(plr)
                        if plr.Character ~= nil then 
                            if getRoot(plr) ~= false then
                                if plr.Character:FindFirstChild('Head') ~= nil then
                                    if plr.Character:FindFirstChild('Humanoid') ~= nil then
                                        return true
                                    else
                                        return false
                                    end
                                else
                                    return false
                                end
                            else
                                return false
                            end
                        else
                            return false
                        end
                        return false
                    end
                    function getImage(image)
                        local IMAGENS = {
                            {
                                {'Sino', 'Bell'},
                                "rbxthumb://type=Asset&id=5107182114&w=150&h=150"
                            },
                            {
                                {'Sino2', 'Bell2'},
                                "rbxthumb://type=Asset&id=5107182114&w=150&h=150"
                            }
                        }
    
                        for i,im in pairs(IMAGENS) do
                            
                            for i,v in pairs(im[1]) do
                                if image == v then
                                    return im[2]
                                end
                            end
                        end
                    end
                    local CanRefresh = true
                    function RefreshPrisonlife()
                        if CanRefresh then
                            CanRefresh = false
                            local OldPos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                            local OldPos2 = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
                            local camera = workspace.CurrentCamera
                            local oldCameraPos = camera.CFrame
                            spawn(function()
                                game.Players.LocalPlayer.CharacterAdded:wait():WaitForChild('HumanoidRootPart')
                                repeat
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(OldPos2 + Vector3.new(0, 2, 0))
                                    task.wait()
                                until (getRoot(speaker).Position - OldPos2).Magnitude <= 10
                                CanRefresh = true
                                return true
                            end)
                            workspace.Remote.loadchar:InvokeServer(game:GetService("Players").LocalPlayer)
                        end
                    end
                    function KillPrison(player)
                        if typeof(player) == 'Instance' then
                            if player.TeamColor == speaker.TeamColor then
                                workspace.Remote.TeamEvent:FireServer('Medium stone grey')
                                task.wait()
                            end
                            target = player
                            speaker = game.Players.LocalPlayer
                            if speaker.Backpack:FindFirstChild('Remington 870') or speaker.Character:FindFirstChild('Remington 870') then else
                                local args = {
                                    [1] = workspace.Prison_ITEMS.giver:FindFirstChild("Remington 870").ITEMPICKUP
                                }
                    
                                workspace.Remote.ItemHandler:InvokeServer(unpack(args))
                            end
                            
                            local Remington_870 = speaker.Backpack:FindFirstChild('Remington 870') or speaker.Character:FindFirstChild('Remington 870')
                
                            Player = target
                            local FireEvent = {
                                [1] = {
                                    ["RayObject"] = Ray.new(Vector3.new(), Vector3.new()), 
                                    ["Distance"] = 0, 
                                    ["Cframe"] = CFrame.new(), 
                                    ["Hit"] = workspace[Player.Name].Head
                                }, [2] = {
                                    ["RayObject"] = Ray.new(Vector3.new(), Vector3.new()), 
                                    ["Distance"] = 0, 
                                    ["Cframe"] = CFrame.new(), 
                                    ["Hit"] = workspace[Player.Name].Head
                                }, [3] = {
                                    ["RayObject"] = Ray.new(Vector3.new(), Vector3.new()), 
                                    ["Distance"] = 0, 
                                    ["Cframe"] = CFrame.new(), 
                                    ["Hit"] = workspace[Player.Name].Head
                                }, [4] = {
                                    ["RayObject"] = Ray.new(Vector3.new(), Vector3.new()), 
                                    ["Distance"] = 0, 
                                    ["Cframe"] = CFrame.new(), 
                                    ["Hit"] = workspace[Player.Name].Head
                                }, [5] = {
                                    ["RayObject"] = Ray.new(Vector3.new(), Vector3.new()), 
                                    ["Distance"] = 0, 
                                    ["Cframe"] = CFrame.new(), 
                                    ["Hit"] = workspace[Player.Name].Head
                                }, [6] = {
                                    ["RayObject"] = Ray.new(Vector3.new(), Vector3.new()), 
                                    ["Distance"] = 0, 
                                    ["Cframe"] = CFrame.new(), 
                                    ["Hit"] = workspace[Player.Name].Head
                                }, [7] = {
                                    ["RayObject"] = Ray.new(Vector3.new(), Vector3.new()), 
                                    ["Distance"] = 0, 
                                    ["Cframe"] = CFrame.new(), 
                                    ["Hit"] = workspace[Player.Name].Head
                                }, [8] = {
                                    ["RayObject"] = Ray.new(Vector3.new(), Vector3.new()), 
                                    ["Distance"] = 0, 
                                    ["Cframe"] = CFrame.new(), 
                                    ["Hit"] = workspace[Player.Name].Head
                                }
                            }
        
                            
                            if Remington_870.Parent == speaker.Backpack then
                                game:GetService("ReplicatedStorage").ShootEvent:FireServer(FireEvent, game.Players.LocalPlayer.Backpack[Remington_870.Name])
                                game:GetService("ReplicatedStorage").ReloadEvent:FireServer(game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Remington 870"))
                            elseif Remington_870.Parent == speaker.Character then
                                game:GetService("ReplicatedStorage").ShootEvent:FireServer(FireEvent, game.Players.LocalPlayer.Character[Remington_870.Name])
                                game:GetService("ReplicatedStorage").ReloadEvent:FireServer(game:GetService("Players").LocalPlayer.Character:FindFirstChild("Remington 870"))
                            end
                            spawn(function()
                                wait(.1)
                                task.spawn(function()

                                end)
                                CriminalP()
                            end)
                        
                        elseif typeof(player) == 'table' then
                            
                            target = player
                            speaker = game.Players.LocalPlayer
                            if speaker.Backpack:FindFirstChild('Remington 870') or speaker.Character:FindFirstChild('Remington 870') then else
                                local args = {
                                    [1] = workspace.Prison_ITEMS.giver:FindFirstChild("Remington 870").ITEMPICKUP
                                }
                    
                                workspace.Remote.ItemHandler:InvokeServer(unpack(args))
                            end
                            
                            local Remington_870 = speaker.Backpack:FindFirstChild('Remington 870') or speaker.Character:FindFirstChild('Remington 870')
                
    
                            local FireEvent = {
                                
                            }
                            for i,v in pairs(player) do
                                if v.Character and v.Character:FindFirstChild('Head') ~= nil then
                                    for i = 1,8 do
                                        FireEvent[#FireEvent + 1] = {
                                            ["RayObject"] = Ray.new(Vector3.new(), Vector3.new()), 
                                            ["Distance"] = 0, 
                                            ["Cframe"] = CFrame.new(), 
                                            ["Hit"] = v.Character.Head
                                        }
                                    end
                                end
                            end
                            if Remington_870.Parent == speaker.Backpack then
                                game:GetService("ReplicatedStorage").ShootEvent:FireServer(FireEvent, game.Players.LocalPlayer.Backpack[Remington_870.Name])
                                game:GetService("ReplicatedStorage").ReloadEvent:FireServer(game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Remington 870"))
                            elseif Remington_870.Parent == speaker.Character then
                                game:GetService("ReplicatedStorage").ShootEvent:FireServer(FireEvent, game.Players.LocalPlayer.Character[Remington_870.Name])
                                game:GetService("ReplicatedStorage").ReloadEvent:FireServer(game:GetService("Players").LocalPlayer.Character:FindFirstChild("Remington 870"))
                            end
                            spawn(function()
                                wait(.1)
                                task.spawn(function()
                                    for i,v in pairs(player) do

                                    end
                                end)
                                CriminalP()
                            end)
                        end
                    end
                    function FindTool(toolName, plr)
                        plr = plr or game.Players.LocalPlayer
                        backpack = plr.Backpack
                        char = plr.Character
                    
                        if backpack:FindFirstChild(toolName) then
                            return backpack:FindFirstChild(toolName)
                        elseif char:FindFirstChild(toolName) then
                            return char:FindFirstChild(toolName) 
                        else
                            return false;                       
                        end
                    end
                    
                    local cannotifyv2 = true
                    function NotifyV2(title, text, duration)
                        if cannotifyv2 == false then repeat wait() until cannotifyv2 == true end
                        cannotifyv2 = false
                        local BlackGui = Instance.new("ScreenGui")
                        local Frame = Instance.new("Frame")
                        local TextLabel = Instance.new("TextLabel")
                        local TextLabel_2 = Instance.new("TextLabel")
                        
                        BlackGui.Name = "BlackGui"
                        BlackGui.Parent = game.CoreGui
                        BlackGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
                        BlackGui.IgnoreGuiInset = true
                        
                        Frame.Parent = BlackGui
                        Frame.AnchorPoint = Vector2.new(0.5, 0.5)
                        Frame.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
                        Frame.BorderSizePixel = 0
                        Frame.Position = UDim2.new(0.5, 0, 0.5, 0)
                        Frame.Size = UDim2.new(1, 0, 1, 0)
                        
                        TextLabel.Parent = Frame
                        TextLabel.AnchorPoint = Vector2.new(0.5, 0)
                        TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                        TextLabel.BackgroundTransparency = 1.000
                        TextLabel.Position = UDim2.new(0.5, 0, 0.0759999976, 0)
                        TextLabel.Size = UDim2.new(0.535663366, 0, 0.150000006, 0)
                        TextLabel.Font = Enum.Font.SourceSansBold
                        TextLabel.Text = title
                        TextLabel.TextColor3 = Color3.fromRGB(225, 225, 225)
                        TextLabel.TextSize = 60.000
                        
                        TextLabel_2.Parent = Frame
                        TextLabel_2.AnchorPoint = Vector2.new(0.5, 0.5)
                        TextLabel_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                        TextLabel_2.BackgroundTransparency = 1.000
                        TextLabel_2.Position = UDim2.new(0.5, 0, 0.575156569, 0)
                        TextLabel_2.Size = UDim2.new(0.535663366, 0, 0.150000006, 0)
                        TextLabel_2.Font = Enum.Font.SourceSansBold
                        TextLabel_2.Text = text
                        TextLabel_2.TextColor3 = Color3.fromRGB(225, 225, 225)
                        TextLabel_2.TextSize = 45.000
                        task.spawn(function()
                            for i,v in pairs(game:GetDescendants()) do
                                if v.ClassName == 'ScreenGui' and v ~= BlackGui then
                                    local oldEnabled = v.Enabled 
                                    v.Enabled = false
                                    task.spawn(function()
                                        wait(duration)
                                        cannotifyv2 = true
                                        v.Enabled = oldEnabled
                                    end)
                                end 
                            end
                            wait(duration)
                            BlackGui:Destroy()
                        end)
                    end
                    
                    function getHumanoid(plr)
                        if plr.Character ~= nil then
                            if plr.Character:FindFirstChildWhichIsA('Humanoid') then
                                return plr.Character:FindFirstChildWhichIsA('Humanoid');
                            else
                                return false;
                            end
                        else
                            return false
                        end
                    end
                    function getHead(plr)
                        if plr.Character ~= nil then
                            if plr.Character:FindFirstChild('Head') then
                                return plr.Character:FindFirstChild('Head');
                            else
                                return false;
                            end
                        else
                            return false
                        end
                    end
                    function getRoot(plr)
                        if plr.Character ~= nil then
                            if plr.Character:FindFirstChild('HumanoidRootPart') then
                                return plr.Character:FindFirstChild('HumanoidRootPart');
                            else
                                return false;
                            end
                        else
                            return false
                        end
                    end
                    function getTorso(plr)
                        if plr.Character ~= nil then
                            if plr.Character:FindFirstChild('Torso') then
                                return plr.Character:FindFirstChild('Torso');
                            elseif plr.Character:FindFirstChild('UpperTorso') then
                                return plr.Character:FindFirstChild('UpperTorso')
                            else
                                return false;
                            end
                        else
                            return false
                        end
                    end
                    
                    _G.Error = false
                    local Sounds = Instance.new('Folder', workspace)
                    Sounds.Name = 'Magnatta Sounds'
                    local SucessSound = Instance.new('Sound', Sounds)
                    --Eu
                    --Minha best linda
                    SucessSound.SoundId = 'rbxassetid://9074670249'

                    function StartError()
                        if _G.Error == false then
                            _G.Error = true
                            local Sound = Instance.new('Sound', workspace)
                            Sound.Name = 'Error'
                            Sound.SoundId = 'rbxassetid://6213876119'
                            Sound.Volume = 10
                            Sound.Looped = true
                            Sound:Play()
                            repeat
                                wait()
                            until _G.Error == false
                            Sound:Destroy()
                        end
                    end
                    function StopError()
                        _G.Error = false
                    end
