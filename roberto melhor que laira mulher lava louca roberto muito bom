                                            if (getCharacter(target).UpperTorso.Position - oldpos.Position).Magnitude > 10 then
                                                repeat
                                                    getRoot(speaker).CFrame = CFrame.new(getCharacter(target).UpperTorso.Position + Vector3.new(0, 1.4, 0))
                                                    wait(.5)
                                                until (getRoot(speaker).Position - getCharacter(target).UpperTorso.Position).Magnitude < 5 or getCharacter(target).BodyEffects['K.O'].Value == false

                                                Grabbing(true)

                                                getRoot(speaker).CFrame = oldpos
                                                wait(.8)
                                                Grabbing(false)
                                            end
                                        end
                                        force()
                                    end
                                else
                                    if getRoot(speaker) then getRoot(speaker).CFrame = oldpos end
                                    wait()
                                    if getRoot(speaker) then getRoot(speaker).CFrame = oldpos end
                                    wait()
                                    if getRoot(speaker) then getRoot(speaker).CFrame = oldpos end

                                end
                            end)
