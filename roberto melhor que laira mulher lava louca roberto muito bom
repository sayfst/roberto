                    function DeleteHumanoid()
                        if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
                            game.Players.LocalPlayer.Character.Humanoid.Name = 1
                            local clone = game.Players.LocalPlayer.Character["1"]:Clone()
                            game.Players.LocalPlayer.Character["1"]:Destroy()
                            game.Players.LocalPlayer.Character.Animate.Disabled = true
                            wait()
                            clone.Parent = game.Players.LocalPlayer.Character
                            clone.Name = "Humanoid"
                        end
                    end


                    Player:CreateTextLabel('Speed')

                    Player:CreateToggle('Anti cheat', function(bool)
                        if bool then
                            getHumanoid(speaker).Name = 'Cheated'
                        else
                            getHumanoid(speaker).Name = 'Humanoid'
                        end
                    end)
                    
                    Player:CreateButton('Speed', function()
                        local gmt = getrawmetatable(game)
                    
                        local oldindex = gmt.__index
                    
                        setreadonly(gmt, false)
                        gmt.__index = newcclosure(function(self, b)
                            if self == 'Humanoid' then
                                return 16
                            end
                            return oldindex(self, b)
                        end)
                        setreadonly(gmt, true)
                        getHumanoid(speaker).WalkSpeed = set_speed
                    end)
                    
                    Player:CreateSlider('Set speed', 0, 100, function(value)
                        set_speed = value
                    end)

                    Player:CreateTextLabel('Others')
                    Player:CreateButton('God mode', function()
                        DeleteHumanoid()
                    end)
                    Player:CreateButton('Reset', function()
                        game.Players.LocalPlayer.Character.Humanoid.Health = 0
                    end)
    
