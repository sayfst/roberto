                            local Parachute

                            for i,v in pairs(getgc()) do
                                if type(v) == 'function' and getinfo(v).name then
                                    if getinfo(v).name == 'Parachute' then
                                        Parachute = v
                                    end
                                end
                            end
                            function tp(vector, customvelo, isplayer, faltando)
                                if typeof(vector) == 'table' then
                                    vector = Vector3.new(vector[1], vector[2], vector[3])
                                end
                                customvelo = customvelo or 5e4
                                faltando = faltando or false
                                isplayer = isplayer or false
                                if isplayer == true then
                                    Notify(
                                        'Follow Player',
                                        'Following '..target.DisplayName,
                                        nil,
                                        5
                                    )
                                end
                                abort = false
                                if customvelo == nil then
                                    customvelo = 150000
                                end
                                local aperta = false
                                



                                local BodyVelocity = Instance.new('BodyPosition')
                                BodyVelocity.MaxForce = Vector3.new(9e9, 9e9, 9e9)
                                BodyVelocity.D = 500
                                BodyVelocity.P = 50000
                                BodyVelocity.Parent = getRoot(speaker)
                                local reached = false
                                local old = teleport_velo
                                local Loop = game.RunService.Stepped:Connect(function(_, step)
                                    local yourRoot = getRoot(speaker)
                                    local diff
                                    if isplayer == true then
                                        vector = getRoot(target).Position + Vector3.new(0, 200, 0)
                                    end
                                    diff = (vector - getRoot(speaker).Position)
                                    
                                    BodyVelocity.Position = getRoot(speaker).Position + diff.Unit * math.min(step * teleport_velo, diff.Magnitude)
                                    if speaker.Character:FindFirstChild('Humanoid').Sit == true then
                                        BodyVelocity.P = customvelo
                                        teleport_velo = 400
                                    else
                                        teleport_velo = 180
                                        BodyVelocity.P = 50000
                                    end
                                    if (vector - getRoot(speaker).Position).Magnitude < 15 then
                                        if getHumanoid(speaker).Sit == false then
                                            Parachute()
                                        end
                                    end
                                    if (vector - getRoot(speaker).Position).Magnitude < 1 then
                                        reached = true
                                    end
                                end)

                                repeat wait() until reached == true or abort == true
                                delay(1, function()
                                    StopParachute()
                                end)
                                Loop:Disconnect()
                                BodyVelocity:Destroy()
                                getRoot(speaker).Velocity = Vector3.new(0, 3, 0)
                                
                            end

                            function Teleport(pos, isup)

                                isup = isup or false
                                if configs.car_to_teleport == true and not getHumanoid(speaker).Sit == true then
                                    Notify('Teleportar', 'O jogador nao esta em um carro...', nil, 3)
                                    wait(0.1)
                                    Notify('Teleportar', 'Procurando um Carro...', nil, 3)
                                    wait()
                                    if getCamaro() ~= nil then
                                        Notify('Teleportar', 'Carro Encontrado', nil, 3)
                                        getRoot(speaker).CFrame = getRoot(speaker).CFrame + Vector3.new(0, 200, 0)
                                        wait()
                                        
                                        tp(getCamaro().Engine.Position + Vector3.new(0, 200, 0), 50000)
                                        local enterVehicle

                                        for i,v in pairs(getgc()) do
                                            if type(v) == 'function' and  getinfo(v).name then
                                                if getinfo(v).name == 'InputBegan' then
                                                    enterVehicle = v
                                                end
                                            end
                                        end
                                        local funcaoEntrar
                                        for i,v in pairs(getupvalues(enterVehicle)) do
                                            if i == 2 then
                                                funcaoEntrar = v
                                            end
                                        end
                                        local fire
                                        for i,v in pairs(getgc()) do
                                            if type(v) == 'function' and getinfo(v).name == 'AttemptVehicleEnter' then
                                                fire = v
                                            end
                                        end

                                        local tag
                                        for i,v in pairs(getupvalues(fire)) do
                                            if i == 2 then
                                                for i,v in pairs(v) do
                                                    for i,v in pairs(getupvalues(v)) do
                                                        if i == 1 then
                                                            for i,v in pairs(getupvalues(v)) do
                                                                if i == 2 then
                                                                    tag = tostring(v)
                                                                end
                                                            end
                                                        end
                                                    end
                                                end
                                            end
                                        end

                                        local time = 0
                                        task.spawn(function()
                                            wait(0.5)
                                            
                                            
                                            repeat
                                                keypress(keycodeMap['e'])

                                                wait(0.1)
                                                keyrelease(keycodeMap['e'])
                                                wait(0.1)
                                                time = time + 0.2
                                            until getHumanoid(speaker).Sit == true or getHumanoid(speaker).Health == 0 or time > 3
                                        end)
                                        StopParachute() 
                                        repeat
                                            task.wait()
                                            getRoot(speaker).Velocity = Vector3.new(0, 3, 0)
                                            getRoot(speaker).CFrame = CFrame.new(getCamaro().Seat.CFrame * CFrame.new(-6.5, 7, 0).Position)
                                            local camera = workspace.CurrentCamera
                                            camera.CFrame = CFrame.new(camera.CFrame.Position, getCamaro().Seat.Position)
                                                                                    
                                            
                                            
                                        until getHumanoid(speaker).Sit == true or getHumanoid(speaker).Health == 0 or time > 3
                                        
                                        
                                        if getHumanoid(speaker).Health == 0 then for i = 1,5 do execCmd('Abort teleport') end return end
                                    else
                                        Notify('Teleportar', 'Nenhum Carro Disponivel...', nil, 3)
                                    end
                                end

                                if getHumanoid(speaker).Sit == false then
                                    getRoot(speaker).CFrame = getRoot(speaker).CFrame + Vector3.new(0, 100, 0)
                                    wait(0.1)
                                    getRoot(speaker).CFrame = getRoot(speaker).CFrame + Vector3.new(0, 100, 0)
                                    local canStop = false
                                    task.spawn(function()
                                        while canStop == false do
                                            if getHumanoid(speaker).Sit == false then
                                                local args = {
                                                    [1] = "Chassis",
                                                    [2] = "Camaro"
                                                }

                                                game:GetService("ReplicatedStorage").GarageSpawnVehicle:FireServer(unpack(args))
                                            end
                                            task.wait(1)
                                        end
                                    end)
                                    wait()
                                    
                                    tp(pos + Vector3.new(0, 200, 0), 150000, isup)
                                    
                                    wait()
                                    canStop = true
                                    tp(pos, 150000, isup)
                                else
                                    tp(getRoot(speaker).Position + Vector3.new(0, 200, 0), 50000)
                                    wait()
                                    local canStop = false
                                    task.spawn(function()
                                        while canStop == false do
                                            if getHumanoid(speaker).Sit == false then
                                                local args = {
                                                    [1] = "Chassis",
                                                    [2] = "Camaro"
                                                }

                                                game:GetService("ReplicatedStorage").GarageSpawnVehicle:FireServer(unpack(args))
                                            end
                                            task.wait()
                                        end
                                    end)
                                    tp(pos + Vector3.new(0, 200, 0), 150000, isup)
                                    wait()
                                    canStop = true
                                    tp(pos, 50000, isup, faltando)
                                end
                                
                            end
                            
                            
                            
