                                local SelectionBox = Instance.new('SelectionBox', v.Handle)
                                SelectionBox.Adornee = v.Handle
                                SelectionBox.Color3 = Color3.new(1,0,0)
                            end 
                            end
                        end
                    end)
                    
                    Player:CreateButton('Player Invisivel', function()
                        local player = game.Players.LocalPlayer
                        
                        local char = player.Character or player.CharacterAdded:wait()
                        local humanoid = char:WaitForChild('Humanoid')
                        local lowerTorso = char:WaitForChild('LowerTorso')
                        oldposss = humanoid.RootPart.CFrame
                        humanoid.RootPart.CFrame = CFrame.new(0, 100, 0)
                        
                        wait(.1)
                        
                        lowerTorso:Destroy()
                        
                        for i,v in pairs(char:GetChildren()) do
                            if v:IsA('BasePart') and v.Name ~= humanoid.RootPart.Name then
                                task.spawn(function()
                                    repeat
                                        v.CFrame = CFrame.new(0, 100, 0)
                                        v.Velocity = Vector3.new(0, 5, 0)
                                        wait()
                                    until humanoid.Health == 0
                                end)
                            end
                        end
                        humanoid.RootPart.CFrame = oldposss
                    end)
                    Player:CreateButton('Player visivel', function()
                        Refresh()
                        wait()
                        speaker.CharacterAdded:wait()
                        repeat wait() until getRoot(speaker)
                        wait(.1)
                        getRoot(speaker).CFrame = oldposss
                    end)
                    FLYSPEED = 25
                    local ADDED = 50
                    VELOCIDADE = 10
                    Player:CreateSlider('Noclip X speed', 1, 200, function(bool)
                        FLYSPEED = bool
                    end, true)
                    Player:CreateSlider('Shift Added Noclip X speed', 1, 200, function(bool)
                        ADDED = bool
                    end, true)
                    Player:CreateSlider('Noclip Q speed', 1, 10, function(bool)
                        VELOCIDADE = bool
                    end, true)
                    Player:CreateSlider('Noclip Tool speed', 1, 500, function(bool)
                        TOOLSPEED = bool
                    end)
                    Player:CreateToggle('Noclip Tool (Force Auto Stop on Unequip Tool)', function(b)
                        AutoStop = b 
                    end)
                    
                    local UserInputService = game.UserInputService

                    UserInputService.InputBegan:Connect(function(input,ischat)

                        if input.KeyCode == Enum.KeyCode.LeftShift then
                            FLYSPEED = FLYSPEED + ADDED
                        end
                    end)
                    UserInputService.InputEnded:Connect(function(input,ischat)

                        if input.KeyCode == Enum.KeyCode.LeftShift then
                            FLYSPEED = FLYSPEED - ADDED
                        end
                    end)
                
                    local nc_x = {
                        auto_spin = false;
                        auto_gyro = false;
                    }
                    Player:CreateButton('Noclip (X)', function()
                        function startFly()
                            repeat wait() until game.Players.LocalPlayer and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:findFirstChild("Head") and game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Humanoid") local mouse = game.Players.LocalPlayer:GetMouse() repeat wait() until mouse local plr = game.Players.LocalPlayer local torso = plr.Character.Head local flying = false local deb = true local ctrl = {f = 0, b = 0, l = 0, r = 0} local lastctrl = {f = 0, b = 0, l = 0, r = 0} local maxspeed = 5000 local speed = 5000 
                            function Fly() 
                                local bg = Instance.new("BodyGyro", torso) 
                                bg.P = 0
                                bg.maxTorque = Vector3.new(9e9, 9e9, 9e9) 
                                bg.cframe = torso.CFrame 

                                local Spin

                                if nc_x.auto_spin == true then
                                    Spin = Instance.new("BodyAngularVelocity")
                                    Spin.Name = "Spinning"
                                    Spin.Parent = getRoot(speaker)
                                    Spin.MaxTorque = Vector3.new(0, math.huge, 0)
                                    
                                end
                                local bv = Instance.new("BodyVelocity", torso) 
                                bv.velocity = Vector3.new(0,0.1,0) 
                                bv.maxForce = Vector3.new(9e9, 9e9, 9e9) 
                                repeat wait() 
                                    if nc_x.auto_spin and Spin ~= nil then
                                        Spin.AngularVelocity = Vector3.new(0,9999999999999,0)
                                    end
                                    if nc_x.auto_gyro == true then
                                        bg.P = 5000
                                        bg.cframe = workspace.CurrentCamera.CoordinateFrame
                                    else
                                        bg.P = 0
                                    end
                                    
                                    plr.Character:FindFirstChildWhichIsA('Humanoid').PlatformStand = false 
                                    if ctrl.l + ctrl.r ~= 100000 or ctrl.f + ctrl.b ~= 10000 then speed = speed+.0+(speed/maxspeed) if speed > maxspeed then speed = maxspeed end elseif not (ctrl.l + ctrl.r ~= 5 or ctrl.f + ctrl.b ~= 5) and speed ~= 5 then speed = speed-5 if speed > 5 then speed = -2 end end if (ctrl.l + ctrl.r) ~= 5 or (ctrl.f + ctrl.b) ~= 5 then bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (ctrl.f+ctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(ctrl.l+ctrl.r,(ctrl.f+ctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed lastctrl = {f = ctrl.f, b = ctrl.b, l = ctrl.l, r = ctrl.r} elseif (ctrl.l + ctrl.r) == 5 and (ctrl.f + ctrl.b) == 5 and speed ~= 5 then bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (lastctrl.f+lastctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(lastctrl.l+lastctrl.r,(lastctrl.f+lastctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed else 
                                    bv.velocity = Vector3.new(0,0.1,0) end 
                                    
                                until not flying 
                                if Spin ~= nil then
                                    Spin:Destroy()
                                end
                                ctrl = {f = 0, b = 0, l = 0, r = 0} lastctrl = {f = 0, b = 0, l = 0, r = 0} speed = 5 bg:Destroy() bv:Destroy() plr.Character:FindFirstChildWhichIsA('Humanoid').PlatformStand = false end mouse.KeyDown:connect(function(key) if key:lower() == "x" then if flying then flying = false spawn(function() game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, 25, 0) end) else flying = true Fly() end elseif key:lower() == "w" then ctrl.f = FLYSPEED elseif key:lower() == "s" then ctrl.b = -FLYSPEED elseif key:lower() == "a" then ctrl.l = -FLYSPEED elseif key:lower() == "d" then ctrl.r = FLYSPEED end end) mouse.KeyUp:connect(function(key) if key:lower() == "w" then ctrl.f = 0 elseif key:lower() == "s" then ctrl.b = 0 elseif key:lower() == "a" then ctrl.l = 0 elseif key:lower() == "d" then ctrl.r = 0 end end) Fly()
                        end
                        startFly()
                        game.Players.LocalPlayer.CharacterAdded:Connect(function(character)
                            repeat wait() until character:FindFirstChild('HumanoidRootPart') ~= nil
                            startFly()
                        end)
                    end)
                    Player:CreateToggle('Noclip X (Auto gyro)', function(toggled)
                        nc_x.auto_gyro = toggled
                    end)
                    
                    Player:CreateButton('Noclip (Tool)', function()
                        repeat wait() until game.Players.LocalPlayer.Character
                        local Enabled = false
                        
                        local Tool = Instance.new('Tool') -- Create a tool
                        Tool.Parent = game.Players.LocalPlayer.Backpack -- Put your tool on backpack
                        Tool.RequiresHandle = false -- Removing Requires handle
                        Tool.Name = 'Fly' -- Tool name (You can rename!)
                        Tool.Equipped:Connect(function()
                            Enabled = true
                            spawn(function()
                                local BodyVelocity = Instance.new('BodyVelocity', game.Players.LocalPlayer.Character.Head)
                                BodyVelocity.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
                                
                                while Enabled == true do
                                    BodyVelocity.Velocity = game.Players.LocalPlayer:GetMouse().Hit.LookVector * TOOLSPEED
                                    wait()
                                end
                            end)
                        end)
                        Tool.Unequipped:Connect(function()
                            Enabled = false
                            game.Players.LocalPlayer.Character.Head:FindFirstChildWhichIsA('BodyVelocity'):Destroy()
                            if AutoStop then
                                getRoot(speaker).Velocity = Vector3.new(0, 0, 0)
                                getRoot(speaker).CFrame = CFrame.new(getRoot(speaker).Position)
                            end
                        end)
                    end)
                    local CanNoclip = false
                    function checkNoclip()
                        if CanNoclip == false then
                            return
                        end
                    end
                    Player:CreateToggle('Noclip', function(b)
                        CanNoclip = b
                    end, true)
                    spawn(function()
                        if _G.NOCLIP_LOADED then
                            return
                        end 
                        _G.NOCLIP_LOADED = true
                        local Enabled = false 
                        local Camera = game.Workspace.CurrentCamera 
                        local Player = game:GetService("Players").LocalPlayer 
                        local Input = game:GetService("UserInputService") 
                        local Forward = false 
                        local Back = false 
                        local Left = false 
                        local Right = false 
                        local Up = false 
                        local Down = false 
                        local function SetPlayer() 
                            if Enabled and CanNoclip then
                            
                                task.spawn(function()
                                    local hrp = game.Players.LocalPlayer.Character.HumanoidRootPart
                                    local part = Instance.new('Part')
                                    part.Parent = getCharacter(speaker)
                                    part.Name = FloatName
                                    part.Anchored = true
                                    part.Size = Vector3.new(15, 1, 15)



                                    while Enabled == true do
                                        part.Position = getRoot(speaker).Position - Vector3.new(0, 3.5, 0)
                                        getRoot(speaker).Velocity = Vector3.new(0, 0, 0)
                                        task.wait()
                                    end
                                    part:Destroy()
                                end)
                            end
                        end 
                        loadstring(game:HttpGet("https://raw.githubusercontent.com/LegoHacker1337/legohacks/main/PhysicsServiceOnClient.lua"))()
                        function nc(t)

                            if CanNoclip then else return end           

                            if t then
                                task.spawn(function()
                                    Noclipping = game.RunService.Stepped:Connect(function()
                                        for _, child in pairs(speaker.Character:GetDescendants()) do
                                            if child:IsA("BasePart") and child.CanCollide == true and child.Name ~= FloatName then
                                                child.CanCollide = false
                                            end
                                        end
                                    end)
                                end)
                            else
                                Noclipping:Disconnect()
                            end

                        end
                        Input.InputBegan:Connect(function(Key,IsChat) 
                            checkNoclip()
                            if IsChat then return end 
                            if Key.KeyCode == Enum.KeyCode.LeftShift then
                                VELOCIDADE = VELOCIDADE + 15
                            end
                            if Key.KeyCode == Enum.KeyCode.Q then 
                                Enabled = not Enabled 
                                nc(Enabled)
                                SetPlayer() 
                            end 
                            if Key.KeyCode == Enum.KeyCode.W or Key.KeyCode == Enum.KeyCode.Up then 
                                Forward = true 
                            end 
                            if Key.KeyCode == Enum.KeyCode.S or Key.KeyCode == Enum.KeyCode.Down then 
                                Back = true 
                            end 
                            if Key.KeyCode == Enum.KeyCode.A or Key.KeyCode == Enum.KeyCode.Left then
                                Left = true 
                            end 
                            if Key.KeyCode == Enum.KeyCode.D or Key.KeyCode == Enum.KeyCode.Right then 
                                Right = true 
                            end 
                            if Key.KeyCode == Enum.KeyCode.Space then 
                                Up = true 
                            end             
                            if Key.KeyCode == Enum.KeyCode.LeftControl then 
                                Down = true 
                            end 
                            
                        end) 
                        Input.InputEnded:Connect(function(Key,IsChat) 
                            checkNoclip()
                            if IsChat then return end 
                            if Key.KeyCode == Enum.KeyCode.LeftShift then
                                VELOCIDADE = VELOCIDADE - 15
                            end
                            if Key.KeyCode == Enum.KeyCode.W or Key.KeyCode == Enum.KeyCode.Up then 
                                Forward = false 
                            end 
                            if Key.KeyCode == Enum.KeyCode.S or Key.KeyCode == Enum.KeyCode.Down then 
                                Back = false 
                            end 
                            if Key.KeyCode == Enum.KeyCode.A or Key.KeyCode == Enum.KeyCode.Left then
                                Left = false 
                            end 
                            if Key.KeyCode == Enum.KeyCode.D or Key.KeyCode == Enum.KeyCode.Right then 
                                Right = false 
                            end 
                            if Key.KeyCode == Enum.KeyCode.Space then 
                                Up = false 
                            end 
                            if Key.KeyCode == Enum.KeyCode.LeftControl then 
                                Down = false 
                            end 
                        end) 
                        spawn(function() 
                            local CanAnchored = true
                            game:GetService("RunService").Heartbeat:Connect(function()
                                if Enabled and CanNoclip then
                                    checkNoclip() 

                                    pcall(function() 
                                        if Forward then 
                                            Player.Character:TranslateBy(Camera.CFrame.lookVector*VELOCIDADE) 
                                        end 
                                        if Back then 
                                            Player.Character:TranslateBy(-Camera.CFrame.lookVector*VELOCIDADE) 
                                        end 
                                        if Left then 
                                            Player.Character:TranslateBy(-Camera.CFrame:vectorToWorldSpace(Vector3.new(1,0,0))*VELOCIDADE) 
                                        end 
                                        if Right then 
                                            Player.Character:TranslateBy(Camera.CFrame:vectorToWorldSpace(Vector3.new(1,0,0))*VELOCIDADE) 
                                        end 
                                        if Up then Player.Character:TranslateBy(Camera.CFrame:vectorToWorldSpace(Vector3.new(0,1,0))*VELOCIDADE) 
                                        end 
                                        if Down then 
                                            Player.Character:TranslateBy(-Camera.CFrame:vectorToWorldSpace(Vector3.new(0,1,0))*VELOCIDADE) 
                                        end 
                                    end) 
                                end 
                            end)
                        end) 
                    end)  
    
                    if game.PlaceId == 155615604 then
                        pcall(function()
                        
                            Player:CreateButton('')
    
                            Player:CreateTextLabel('Prison life')
