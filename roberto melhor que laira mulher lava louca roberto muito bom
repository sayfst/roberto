                            function bringPlayerToPos(...)
                                local function loadChar()
                                    workspace.Remote.loadchar:InvokeServer(game.Players.LocalPlayer)
                                    game.Players.LocalPlayer.Character:WaitForChild('HumanoidRootPart')
                                end
                                
                                local function Hammer()
                                    workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.single.Hammer.ITEMPICKUP)
                                end
                                
                                local function FindPlayer(player)
                                    for i,v in pairs(game.Players:GetPlayers()) do
                                        if v ~= game.Players.LocalPlayer then
                                            if v.Name:lower():match('^'..player:lower()) then
                                                return v;
                                            end
                                        end
                                    end
                                end
                                
                                local function FindHammer()
                                    local lplr = game.Players.LocalPlayer
                                    local HammerTool = lplr.Backpack:FindFirstChild('Hammer') or lplr.Character:FindFirstChild('Hammer') 
                                    if HammerTool then
                                        HammerTool.Parent = lplr.Backpack
                                        return lplr.Backpack:FindFirstChild('Hammer')
                                    else
                                        return false
                                    end
                                end
                                local position = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                                local position2 = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
                                local function humanoid()
                                    if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
                                        game.Players.LocalPlayer.Character.Humanoid.Name = 1
                                        local clone = game.Players.LocalPlayer.Character["1"]:Clone()
                                        game.Players.LocalPlayer.Character["1"]:Destroy()
                                        wait()
                                        clone.Parent = game.Players.LocalPlayer.Character
                                        clone.Name = "Humanoid"
                                    end
                                end
                                
                                local TargetName = target.Name
                                
                                local Target = FindPlayer(TargetName)
                                
                                local TargetToTeleport = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                                
                                if Target ~= nil and Target.Character ~= nil and Target.Character:FindFirstChild('Humanoid') ~= nil and Target.Character:FindFirstChild('HumanoidRootPart') ~= nil and Target.Character.Humanoid.Health > 0 and target.Character.Humanoid.Sit ~= true then
                                    if not FindHammer() then
                                        Hammer()
                                        repeat wait() until FindHammer ~= false
                                    end
                                    local OldPos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    
                                    local Now = CFrame.new(...)
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Now
                                    
                                    wait()
                                    local Tool = FindHammer()
                                    
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = position
                                
                                    HRP = speaker.Character.HumanoidRootPart
                                    humanoid()
                                    Tool.Parent = game.Players.LocalPlayer.Character
                                    noclip2(true)
                                    local time = 0
                                    spawn(function()
                                        for i = 1,4 do
                                            time = time + 1
                                            wait(1)
                                        end
                                        time = 'Stop'
                                    end)
                                    repeat
                                        speaker.Character.HumanoidRootPart.CFrame = Now
                                        --Target.Character.HumanoidRootPart.CFrame = speaker.Character:FindFirstChildWhichIsA('Tool'):FindFirstChild('Handle').CFrame
                                        spawn(function()
                                            firetouchinterest(getRoot(target), speaker.Character:FindFirstChildWhichIsA('Tool'):FindFirstChild('Handle'), 0)
                                            wait(0.01)
                                            firetouchinterest(getRoot(target), speaker.Character:FindFirstChildWhichIsA('Tool'):FindFirstChild('Handle'), 1)
                                        end)
                                        task.wait()
                                    until Tool.Parent == Target.Character or HRP == nil or Tool.Parent ~= speaker.Character or time == 'Stop'
                                    noclip2(false)
                                    loadChar()
                                
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = OldPos
                                    wait()
                                    
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = OldPos
                                    wait()
                                    
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = OldPos
                                    wait()
                                end
                            end
                            
