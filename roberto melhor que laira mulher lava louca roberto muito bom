                            function getPlayer(fov)
                                fov = fov or 99999999999999
                                local target = nil
                                local dist = math.huge
                                
                                local mouse = game.Players.LocalPlayer:GetMouse()
                            
                                local camera = workspace.CurrentCamera
                            
                                for i,v in pairs(game.Players:GetChildren()) do
                                    if v ~= game.Players.LocalPlayer then
                                        if v.Character ~= nil and
                                        v.Character:FindFirstChild('Head') and
                                        v.Character:FindFirstChild('Humanoid').Health > 0 then
                                            local vector, onscreen = camera:worldToViewportPoint(v.Character.Head.Position)
                                            
                                            local mag = (Vector2.new(mouse.X, mouse.Y) - Vector2.new(vector.X, vector.Y)).Magnitude
                            
                                            if onscreen and mag < fov then
                                                fov = mag
                                                target = v
                                            end
                                        end
                                    end
                                end
                            
                                return target
                            end
                            
                            
                            local gmt = getrawmetatable(game)
                            
                            local namecall = gmt.__namecall
                            
                            local Fovv = Drawing.new('Circle')
                            task.spawn(function()
                                while game.RunService.Heartbeat:wait() do
                                    Fovv.Radius = aimconfig.fov
                                    Fovv.Visible = aimconfig.fov_visible
                                    Fovv.Position = Vector2.new(speaker:GetMouse().X, speaker:GetMouse().Y)
                                end
                            end)
                            Fovv.Thickness = 1
                            Fovv.Color = Color3.fromRGB(255, 255, 255)
                            local camera = workspace.CurrentCamera
                            Fovv.Position = Vector2.new(camera.ViewportSize.X / 2, camera.ViewportSize.Y / 2)
                            
                            
                            setreadonly(gmt, false)
                            gmt.__namecall = function(self, ...)
                                local args = {...}
                                local method = getnamecallmethod()
                            
                                if tostring(self) == 'MainEvent' and tostring(method) == 'FireServer' then
                                    if args[1] == 'UpdateMousePos' and aimconfig.enabled == true and getPlayer(aimconfig.fov) ~= nil then
                                        args[2] = getPlayer(aimconfig.fov).Character.Head.Position
                                    
                                        return self.FireServer(self, unpack(args))
                                    end
                                end
                                return namecall(self, ...)
                            end
                            setreadonly(gmt, true)

