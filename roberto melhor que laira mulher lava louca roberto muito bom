                        function cp(info, message, info_color)
                            old_color = '@@WHITE@@'
                            pairs_color = '@@WHITE@@'
                            rconsoleprint('\n')
                            rconsoleprint(pairs_color)  
                            rconsoleprint('[') 
                            rconsoleprint(info_color) 
                            rconsoleprint(info) 
                            rconsoleprint(old_color) 
                            rconsoleprint(pairs_color) 
                            rconsoleprint(']') 
                            rconsoleprint(old_color) 
                            rconsoleprint(' '..message)
                        end

                        cp('ERRO', 'Jogo nao indentificado\n', '@@LIGHT_RED@@')
                        cp('JOGOS', 'Iremos mostrar Os jogos que temos Abaixo.\n\n', '@@LIGHT_CYAN@@')
                        cp('TOTAL', 'Temos no total '..num..' jogos com scripts feitas por mim!\n', '@@LIGHT_CYAN@@')
                        for i,v in pairs(games) do
                            cp(v[1], v[2]..'\n', '@@LIGHT_CYAN@@')
                        end

                        cp('PRONTINHO', 'Seus jogos foram printados com sucesso.', '@@LIGHT_CYAN@@')
                        
                    end
                    
                    getgenv().LAIRA_DEGUB = true
                end
                
                
                
                function ESPERAR_RANDOM(salvar)
                    salvar = salvar or false
                    local numbers = {
                        1.1,1.2,1.3,1.4,1.5,1.6,1.7,1.8,1.9,2,2.1,2.2,2.3,2.4,2.5,2.6,2.7,2.8,2.9,3
                    }
                    local select = numbers[math.random(1, #numbers)]
                    
                    if salvar then
                        getgenv().TimeSelectLoad = select
                    end
                end
                local cors = {
                    '@@BLUE@@',
                    '@@RED@@',
                    '@@YELLOW@@',
                }
                local cor = 1
                local maxcor = #cors

                function CustomPrint(info, message, info_color, old_color, pairs_color)
                    old_color = '@@WHITE@@'
                    rconsoleprint('\n')
                    rconsoleprint(pairs_color)  
                    rconsoleprint('[') 
                    rconsoleprint(info_color) 
                    rconsoleprint(info) 
                    rconsoleprint(old_color) 
                    rconsoleprint(pairs_color) 
                    rconsoleprint(']') 
                    rconsoleprint(old_color) 
                    rconsoleprint(' '..message)
                end

                
                function KEYSYSTEM(ALREADYPUT)
                    
                    rconsoleclear()
                    rconsolename('Laira Menu V3 (Whitelist system)')
                    rconsoleinfo('Laira v3')
                   
                    local Ativado = false
                    
                    
                    
                    if ALREADYPUT == false then
                        CustomPrint('WELCOME', 'Laira Menu v3', '@@LIGHT_CYAN@@', '@@BLUE@@', '@@WHITE@@')
                    end
                    CustomPrint('Whitelist', 'Please Put Your whitelist Key Down\nKey: ', '@@LIGHT_CYAN@@', '@@BLUE@@', '@@WHITE@@')
                    local KEYPUT = rconsoleinput()
                    
                    for i,v in pairs(Keys) do
                        if v == KEYPUT then
                            Ativado = true
                        end
                    end
                    
    
                    if Ativado == true then
                        writefile(SavedKey, game:GetService('HttpService'):JSONEncode({KeyMagnatta = KEYPUT}))
                        rconsoleclear()
                        
                        MAGNATA_ACTIVE()
                    else
                        CustomPrint('Error', 'Invalid Key', '@@LIGHT_RED@@', '@@BLUE@@', '@@WHITE@@')
                        wait(1)
                        KEYSYSTEM(true)
                    end
                end
                
                
                if (readfile and writefile and isfile) then
                    if isfile(SavedKey) then
                        local key = game:GetService('HttpService'):JSONDecode(readfile(SavedKey))
                        local KEYVALID = false
                        for i,v in pairs(Keys) do
                            if v == key.KeyMagnatta then
                                KEYVALID = true
                            end
                        end
                        if KEYVALID == false then
                            KEYSYSTEM(true)
                        elseif KEYVALID == true then
                            MAGNATA_ACTIVE()
                        end
                    else
                        KEYSYSTEM(false)
                    end
                end
            end)
        end
    end
end

mag()
