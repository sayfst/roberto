                                local Items = {}
                                for i,v in pairs(game:GetService("Players").LocalPlayer.Stock:GetDescendants()) do 
                                    if v.Name == 'Flavour' then
                                        if not table.find(Items, v.Value) then
                                            Items[#Items + 1] = v.Value
                                        end
                                    end
                                end
                                local text = ''
                                for i = 1,#Items do
                                    text = text .. '\n' .. Items[i]
                                end
                                NotifyV2('Your Flavours:', text, 4)
                            end)
                            b:CreateButton('Sell', function()
