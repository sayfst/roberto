                    local espsPlayer = {}
                    Freatures:CreateButton('Esp player', function()
                        local Titulo = Drawing.new('Text')
                        espsPlayer[#espsPlayer + 1] = Titulo
                        Titulo.Size = 24
                        Titulo.Center = true
                        local plr = target
                        Titulo.Outline = true
                        Titulo.OutlineColor = Color3.fromRGB(255, 0, 0)
                        Titulo.Font = 3
                        function a()
                            task.spawn(function()
                                while task.wait() do
                                    local v,o = workspace.CurrentCamera:worldToViewportPoint(plr.Character.Head.Position)
                                    if o then
                                        Titulo.Position = Vector2.new(v.X, v.Y + 3)
                                        Titulo.Text = plr.Name .. ' | ' .. math.floor((getRoot(speaker).Position - getRoot(plr).Position).Magnitude) .. ' m'
                                        Titulo.Visible = true
                                    else
                                        Titulo.Visible = false
                                    end
                                end
                            end)
                        end

                        coroutine.wrap(a)()
                    end)
                    Freatures:CreateButton('Remove all esps', function()
