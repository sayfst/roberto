                            local CanBuyEgg = true
                            for i = 1,8 do 
                                SupportGame:CreateWhile('Buy Egg '..i, function()
                                    pcall(function()
                                        game:GetService("ReplicatedStorage").Signals.presentEvent:FireServer("openPresent", i)
                                    end)
