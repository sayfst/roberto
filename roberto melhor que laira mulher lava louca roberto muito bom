                            end
                            --rbxassetid://6745990615
                            --rbxassetid://6770729524
                            b:CreateButton('Set clipboard all musics playing on game (Boombox)', function()
                                local Sounds = {}
                                for i,v in pairs(game.Players:GetChildren()) do
                                    if getCharacter(v) and getCharacter(v).LowerTorso then
                                        if getCharacter(v).LowerTorso.BOOMBOXSOUND then
                                            setclipboard(tostring(getCharacter(v).LowerTorso.BOOMBOXSOUND.SoundId))
                                            Notify('Sound', tostring(getCharacter(v).LowerTorso.BOOMBOXSOUND.SoundId))
                                        end
                                    end
                                end
                                local text = 'Sounds:\n'
                                if #Sounds > 0 then
                                    for i = 1,#Sounds do
                                        text = text .. Sounds[i]..'\n'
                                    end
                                    setclipboard(text)
                                end
                            end)
                            b:CreateButton('Set clipboard Boombox id', function()
                                local v = target
                                if getCharacter(v) and getCharacter(v).LowerTorso then
                                    if getCharacter(v).LowerTorso.BOOMBOXSOUND then
                                        setclipboard(tostring(getCharacter(v).LowerTorso.BOOMBOXSOUND.SoundId))
                                        Notify('Sound', tostring(getCharacter(v).LowerTorso.BOOMBOXSOUND.SoundId))
                                    end
                                end
                            end)
                            function FindSound(target, soundname)
                                for i,v in pairs(getCharacter(target):GetDescendants()) do
                                    if v.Name == soundname then
                                        return true
                                    end
                                end
                                return false
                            end
                            b:CreateToggle('Kill', function(bool, force, is_forced)
                                getgenv().Kill = bool

                                if bool then
                                    local Combat = FindTool('Combat')
                                    local Humanoid = getHumanoid(speaker)
                                    oldpos = getRoot(speaker).CFrame

                                    local BodyEffects = getCharacter(target).BodyEffects
                                    function Stomp()
                                        local args = {
                                            [1] = "Stomp"
                                        }

                                        game:GetService("ReplicatedStorage").MainEvent:FireServer(unpack(args))
                                    end
                                    Humanoid:EquipTool(Combat)
                                    MoveMain(game.Lighting)
                                    repeat
                                        getRoot(speaker).Velocity = Vector3.new(0, -2, 0)
                                        getRoot(speaker).CFrame = CFrame.new(getRoot(target).Position + Vector3.new(7, 10, 0))
                                        firetouchinterest(getCharacter(speaker).RightHand, getCharacter(target).UpperTorso, 0)
                                        if getgenv().Kill == false then
                                            break
                                        end
                                        if BodyEffects['K.O'].Value ~= true then
                                            Combat:Activate()
                                            if autoUncativate then
                                                Combat:deactivate()
                                            end
                                        end
                                        wait()
                                    until BodyEffects['K.O'].Value ~= false or getCharacter(speaker).BodyEffects['K.O'].Value == true
                                    MoveMain(game.ReplicatedStorage)
                                    if getCharacter(speaker).BodyEffects['K.O'].Value == true then
                                        task.spawn(function()
                                            Refresh()
                                        end)
                                        return
                                    end
                                    if getgenv().Kill == true then
                                        repeat
                                            getRoot(speaker).CFrame = CFrame.new(getCharacter(target).UpperTorso.Position + Vector3.new(0,1.4,0))
                                            Stomp()
                                            task.wait()
                                        until FindSound(target, 'BloodSplatter') or getgenv().Kill == false
                                        if getgenv().Kill == true then
                                            force()
                                        end
                                    end
                                else
                                    if getRoot(speaker) then getRoot(speaker).CFrame = oldpos end
                                    wait()
                                    if getRoot(speaker) then getRoot(speaker).CFrame = oldpos end
                                    wait()
                                    if getRoot(speaker) then getRoot(speaker).CFrame = oldpos end

                                end
                            end)
