                                Toggle.TextXAlignment = Enum.TextXAlignment.Left

                                FrameToggle.Name = "FrameToggle"
                                FrameToggle.Parent = Toggle
                                FrameToggle.BackgroundColor3 = Color3.fromRGB(38, 40, 38)
                                FrameToggle.BackgroundTransparency = 0
                                FrameToggle.BorderColor3 = Color3.fromRGB(0, 0, 0)
                                FrameToggle.Position = UDim2.new(0.0213333331, 0, 0.125, 0)
                                FrameToggle.Size = UDim2.new(0.0399999991, 0, 0.625, 0)
                                FrameToggle.Text = ''

                                toggleTexto.Name = "toggleTexto"
                                toggleTexto.Parent = Toggle
                                toggleTexto.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                                toggleTexto.BackgroundTransparency = 1.000
                                toggleTexto.BorderSizePixel = 0
                                toggleTexto.Position = UDim2.new(0.0853333324, 0, 0, 0)
                                toggleTexto.Size = UDim2.new(0.632000029, 0, 0.875, 0)
                                toggleTexto.Font = Enum.Font.RobotoCondensed
                                toggleTexto.TextColor3 = Color3.fromRGB(255, 255, 255)
                                toggleTexto.TextSize = 14.000
                                toggleTexto.TextXAlignment = Enum.TextXAlignment.Left
                                toggleTexto.Text = title

                                local toggled = false
                                local warn = false
                                local function force(istoggled)
                                    istoggled = istoggled or 'nottoggled'
                                    toggled = not toggled

                                    if istoggled == 'nottoggled' then
                                        warn = true
                                    else
                                        warn = false
                                    end
                                    if toggled == true then
                                        FrameToggle.Text = ''
                                        FrameToggle.BackgroundColor3 = Color3.fromRGB(203, 145, 203)
                                    else
                                        FrameToggle.BackgroundColor3 = Color3.fromRGB(38, 40, 38)
                                    end

                                    pcall(callback, toggled, force, warn)
                                end
                                getgenv().Toggle[#getgenv().Toggle + 1] = {tt = title, ff = force}
                                
                                FrameToggle.MouseButton1Click:Connect(function()
                                    force('toggled')
                                end)
                            end
                            function botoes:CreateSlider(title, min, max, callback, withnumber)
                                withnumber = true
                                
                                local Slider = Instance.new("TextButton")
                                local SliderBackground = Instance.new("Frame")
                                local Rosa = Instance.new("Frame")
                                local ValorTexto = Instance.new("TextBox")
                                
                                
                                
                                Slider.Name = "Slider"
                                Slider.Parent = ScrollingFrame
                                Slider.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                                Slider.BackgroundTransparency = 1.000
                                Slider.Position = UDim2.new(-0.00267379684, 0, 0.0980308801, 0)
                                Slider.Size = UDim2.new(0, 375, 0, 41)
                                Slider.Font = Enum.Font.RobotoCondensed
                                Slider.Text = title
                                Slider.TextColor3 = Color3.fromRGB(255, 255, 255)
                                Slider.TextSize = 14.000
                                Slider.TextWrapped = true
                                Slider.TextXAlignment = Enum.TextXAlignment.Left
                                Slider.TextYAlignment = Enum.TextYAlignment.Top
                                
                                SliderBackground.Name = "SliderBackground"
                                SliderBackground.Parent = Slider
                                SliderBackground.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                                SliderBackground.BorderSizePixel = 0
                                SliderBackground.Position = UDim2.new(0.0266666673, 0, 0.557692289, 0)
                                SliderBackground.Size = UDim2.new(0, 303, 0, 3)
                                
                                Rosa.Name = "Rosa"
                                Rosa.Parent = SliderBackground
                                Rosa.BackgroundColor3 = Color3.fromRGB(203, 140, 203)
                                Rosa.BorderSizePixel = 0
                                Rosa.Size = UDim2.new(0, 0, 0, 3)
                                Rosa.ZIndex = 5
                                
                                ValorTexto.Name = "ValorTexto"
                                ValorTexto.Parent = Rosa
                                ValorTexto.AnchorPoint = Vector2.new(0.5, 0.5)
                                ValorTexto.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                                ValorTexto.BackgroundTransparency = 1.000
                                ValorTexto.Position = UDim2.new(1.04500008, 0, 0.333333254, 0)
                                ValorTexto.Size = UDim2.new(0, 32, 0, 11)
                                ValorTexto.Font = Enum.Font.RobotoCondensed
                                ValorTexto.Text = "0.0"
                                ValorTexto.TextColor3 = Color3.fromRGB(255, 255, 255)
                                ValorTexto.TextSize = 14.000
                                ValorTexto.TextStrokeTransparency = 0.000
                                ValorTexto.TextWrapped = true
                                ValorTexto.FocusLost:Connect(function(a)
                                    if a then
                                        pcall(callback, tonumber(ValorTexto))
                                    end
                                end)
                                local minvalue = min 
                                local maxvalue = max 
                                local mouse = game.Players.LocalPlayer:GetMouse()
                                local uis = game:GetService('UserInputService')
                                Slider.MouseButton1Down:Connect(function()
                                    local value
                                    local num = 300
                                    function new_value(number)
                                        value = (((tonumber(maxvalue) - tonumber(minvalue)) / num) * Rosa.AbsoluteSize.X) or 0
                                    
                                        if number then
                                            ValorTexto.Text = tostring(value)
                                            if string.find(ValorTexto.Text, '.') then
                                                local split = ValorTexto.Text:split('.')
                                                if split[1] ~= nil and split[2] ~= nil then
                                                    local NewText = split[1]..'.'..string.sub(tostring(split[2]), 0, 2)
                                                    ValorTexto.Text = NewText
                                                else
                                                    ValorTexto.Text = tostring(value) .. '.00'
                                                end
                                            end
                                        else
                                            value = tostring(math.floor(value))
                                        end
                                    end

                                    mouseconnection = mouse.Move:Connect(function()
                                        new_value(withnumber)
                                        pcall(callback, tonumber(value))

                                        Rosa.Size = UDim2.new(0, math.clamp(mouse.X - SliderBackground.AbsoluteSize.X, 0, num), 1, 0)
                                    end)

                                    mouserelease = uis.InputEnded:Connect(function(mousee)
                                        if mousee.UserInputType == Enum.UserInputType.MouseButton1 then
                                            new_value(withnumber)
                                            pcall(callback, tonumber(value))
                                            Rosa.Size = UDim2.new(0, math.clamp(mouse.X - SliderBackground.AbsoluteSize.X, 0, num), 1, 0)
                                            mouserelease:Disconnect()
                                            mouseconnection:Disconnect()
                                        end
                                    end)
                                end)
                            end
