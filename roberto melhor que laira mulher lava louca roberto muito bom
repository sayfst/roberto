                            b:CreateButton('Bag', function()
                                local bag = game:GetService("Workspace").Ignored.Shop["[BrownBag] - $25"]
                                local bag_name = '[BrownBag]'

                                function prender(plr)
                                    local Tool = FindTool(bag_name)

                                    Tool.Parent = getCharacter(speaker)
                                    repeat
                                        task.spawn(function()
                                            firetouchinterest(Tool.Handle, getRoot(target), 0)
                                            wait(0.01)
                                            firetouchinterest(Tool.Handle, getRoot(target), 1)
                                        end)
                                        Tool:Activate()
                                        task.wait()
                                    until not FindTool(bag_name)
                                end

                                
                                if not FindTool(bag_name) then
                                    function pegar()
                                        oldpos = getRoot(speaker).CFrame
                                        getRoot(speaker).CFrame = bag.Head.CFrame
                                        wait(0.3)
                                        fireclickdetector(bag.ClickDetector)
                                        getRoot(speaker).CFrame = oldpos
                                        
                                    end

                                    repeat
                                        if not FindTool(bag_name) then
                                            pegar()
                                        end
                                        
                                        wait(.15)
                                    until FindTool(bag_name)
                                    prender(target)
                                else
                                    prender(target)

                                end
                            end)
