                                oldpos = getRoot(speaker).CFrame

                                for i,v in pairs(game:GetService("Workspace")["_NPCLocations"]:GetDescendants()) do
                                    
                                if v.Name == 'HasIceCream' and v.Parent:FindFirstChild('PartsLoaded') and v.Parent:FindFirstChild('HumanoidRootPart') ~= nil and v.Value == false and num ~= AutoFarmValue and v.Parent.ToBeDestroyed.Value == false then
