local function PMPRWO_fake_script() -- Primario.LocalScript 
	local script = Instance.new('LocalScript', Primario)

	local Frame = script.Parent
	_G.Style = 'Linear'
	_G.Time = '1'
	
	spawn(function()
		local d = { 'Linear'; 'Sine'; 'Back'; 'Quad'; 'Quart'; 'Quint'; 'Bounce'; 'Elastic'; 'Exponential'; 'Circular'; 'Cubic'; } for i,v in pairs(d) do local g = script.Parent.Frame.NEWSTYLE:Clone() g.Parent = script.Parent.Frame g.Text = v; g.Visible = true g.MouseButton1Click:Connect(function() _G.Style = v end) end 
	end)
	
	script.Parent.OPENSTYLES.MouseButton1Click:Connect(function()
		script.Parent.Frame.Visible = not script.Parent.Frame.Visible
	end)
	
	Frame.Time.FocusLost:Connect(function(a)
		if a then
			_G.Time = Frame.Time.Text
		end
	end)
	local CutScenes = Instance.new('Folder', workspace)
	CutScenes.Name = 'CutScenes'
	

	
	local a = ''
	Frame.CreateNew.MouseButton1Click:Connect(function()
		local camera = workspace.CurrentCamera
		a = a .. '{CFrame = CFrame.new('..tostring(camera.CoordinateFrame)..'); Time = '.._G.Time..'; Style = Enum.EasingStyle.'.._G.Style..'}, '
	end)
	
	Frame.Clear.MouseButton1Click:Connect(function()
		a = ''
	end)
	
	Frame.Finish.MouseButton1Click:Connect(function()
		local anotherText = '{'..a..'}'
		
		local FinishText = 'local Data = '..anotherText..'\n\nlocal camera = workspace.CurrentCamera repeat wait() camera.CameraType = Enum.CameraType.Scriptable until camera.CameraType == Enum.CameraType.Scriptable\nfor i,v in pairs(Data) do\n	local TweenService = game:GetService("TweenService")\n	local camera = workspace.CurrentCamera\n	TweenService:Create(camera, TweenInfo.new(v.Time, v.Style), {CFrame = v.CFrame}):Play()\n	wait(v.Time)\nend\ncamera.CameraType = Enum.CameraType.Custom'
		

		local script = Instance.new('LocalScript', Cutscenes)
		script.Source = FinishText
		if (setclipboard) then
			setclipboard(FinishText)
			rconsoleprint(FinishText..'\n\n\n\n')
		else
			rconsoleprint(FinishText..'\n\n\n\n')
		end
	end)
end
coroutine.wrap(PMPRWO_fake_script)()

Notify('Sucess loaded cutscenes creator', 'made by me, $ samuu#6783', getImage('Bell'), 4)
                    end)
                    Server:CreateToggle('Click teleport', function(bool)
                        if bool then
                            local Mouse = game:GetService('Players').LocalPlayer:GetMouse()
    
                            local Uis = game:GetService('UserInputService')
                            
                            ClickTeleport = Uis.InputBegan:Connect(function(input, chat)
                                if chat then return end
                                if input.UserInputType == Enum.UserInputType.MouseButton1 then
                                    local pos = Mouse.Hit + Vector3.new(0, 2.5, 0)
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(pos.X, pos.Y, pos.Z)
                                end
                            end) 
                        else
                            if ClickTeleport then
                                ClickTeleport:Disconnect()
                            end
                        end
                    end)
