                                TextLabel.TextSize = 14.000
                                TextLabel.TextWrapped = true
                                
                                TextBox.Parent = Frame
                                TextBox.BackgroundColor3 = Color3.fromRGB(38, 38, 38)
                                TextBox.BorderSizePixel = 0
                                TextBox.Position = UDim2.new(0.121212125, 0, 0.153392345, 0)
                                TextBox.Size = UDim2.new(0.75757575, 0, 0.0648967549, 0)
                                TextBox.Font = Enum.Font.SourceSans
                                TextBox.PlaceholderText = "Filter Guns"
                                TextBox.Text = ""
                                TextBox.TextColor3 = Color3.fromRGB(255, 255, 255)
                                TextBox.TextSize = 14.000
                                
                                ScrollingFrame.Parent = Frame
                                ScrollingFrame.Active = true
                                ScrollingFrame.BackgroundColor3 = Color3.fromRGB(68, 68, 68)
                                ScrollingFrame.BorderSizePixel = 0
                                ScrollingFrame.Position = UDim2.new(0.087121211, 0, 0.262536883, 0)
                                ScrollingFrame.Size = UDim2.new(0.825757563, 0, 0.693215311, 0)
                                ScrollingFrame.AutomaticCanvasSize = Enum.AutomaticSize.Y
                                
                                UIListLayout.Parent = ScrollingFrame
                                UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
                                
                                TextBox.Changed:Connect(function(b)
                                    if b == 'Text' then
                                        for i,v in pairs(ScrollingFrame:GetChildren()) do
                                            if v.ClassName == 'TextButton' then
                                                if string.find(v.Text:lower(), TextBox.Text:lower()) then
                                                    v.Visible = true
                                                else
                                                    v.Visible = false
                                                end
                                            end
                                        end
                                    end
                                end)
                                
                                for i,v in pairs(game:GetService("Workspace").Ignored.Shop:GetChildren()) do
                                    if v:IsA('Model') then
                                        local TextButton = Instance.new("TextButton")
                                        TextButton.Parent = ScrollingFrame
                                        TextButton.BackgroundColor3 = Color3.fromRGB(49, 49, 49)
                                        TextButton.BorderSizePixel = 0
                                        TextButton.Size = UDim2.new(1, 0, 0, 28)
                                        TextButton.Font = Enum.Font.SourceSans
                                        TextButton.Text = v.Name
                                        TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
                                        TextButton.TextSize = 14.000   
                                        TextButton.Visible = true   
                                        local cooldown = true  
                                        local clicking = false
                                        TextButton.MouseButton1Click:Connect(function()
                                            if cooldown == true then
                                                cooldown = false
                                                local oldposition = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Head.CFrame
                                                wait(0.3)
                                                fireclickdetector(v.ClickDetector, 0)
                                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = oldposition
                                                cooldown = true
                                            end
                                        end)
                                        TextButton.MouseButton2Click:Connect(function()
                                            if clicking == false then
                                                clicking = true
                                                task.spawn(function()
                                                    repeat
                                                        if cooldown == true then
                                                            cooldown = false
                                                            local oldposition = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                                                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Head.CFrame
                                                            wait(0.3)
                                                            fireclickdetector(v.ClickDetector, 0)
                                                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = oldposition
                                                            cooldown = true
                                                        end
                                                        task.wait()
                                                    until clicking ==false
                                                end)
                                            else
                                                clicking = false
                                            end
                                        end)
                                    end
                                end
                            end)
                            
                        end
