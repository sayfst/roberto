                    Etc:CreateButton('Server hop', function()
                        local x = {}
                        for _, v in ipairs(game:GetService("HttpService"):JSONDecode(game:HttpGetAsync("https://games.roblox.com/v1/games/" .. game.PlaceId .. "/servers/Public?sortOrder=Asc&limit=100")).data) do
                            if type(v) == "table" and v.maxPlayers > v.playing and v.id ~= game.JobId then
                                x[#x + 1] = v.id
                            end
                        end
                        if #x > 0 then
                            game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, x[math.random(1, #x)])
                        else
                            print('error')
                        end
                    end)
                
                    Etc:CreateTextLabel('Extra Functions')
                    local fileName = 'Buttons.lua'
                    if isfile(fileName) then else
                        writefile(fileName,game:GetService('HttpService'):JSONEncode({}))
                    end
                    Etc:CreateTextBox('Create Button, Example: Button text;Source', 'String', function(text)
                        
                        
                        text = text:split(';')
                        
                        Etc:CreateButton(text[1], function()
                            pcall(function()                                
                                loadstring(text[2])()
                            end)
                        end)
                        NotifyCallback(
                            'Save that function?',
                            text[1],
                            getImage('Bell'),
                            5,
                            'Save',
                            'No',
                            function()
                                text[2] = string.gsub(text[2], ';', ' ')
                                
                                local HttpService = game:GetService('HttpService')
                                local sp = HttpService:JSONDecode(readfile(fileName))
                                if sp == nil and typeof(sp) ~= 'table' then
                                    sp = {}
                                end
                                sp[#sp + 1] = {
                                    tt=text[1];
                                    ff=text[2];
                                }
                                writefile(fileName, HttpService:JSONEncode(sp))
                            end,
                            function()end
                        )
                    end)
                    Etc:CreateButton('Load All functions saved', function()
                        local sp = game:GetService('HttpService'):JSONDecode(readfile(fileName))

                        for i,v in pairs(sp) do
                            if v.tt and v.ff then
                                Etc:CreateButton(v.tt, function()
                                    pcall(function()
                                        loadstring(v.ff)()
                                    end)
                                end)
                            end
                        end
                    end)
