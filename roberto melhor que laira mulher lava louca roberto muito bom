                            b:CreateToggle('MoneyEsp', function(b)
                                moneyesp = b 
                                if b then
                                    function LoopFunction()
                                        for i,v in pairs(game.Workspace.Ignored.Drop:GetChildren()) do
                                            if v.Name == 'MoneyDrop' then
                                                v.BillboardGui.AlwaysOnTop = true
                                                v.BillboardGui.Size = UDim2.new(0,25,0,25)
                                                end
                                            end
                                    end
                                    function NoLoop()
                                        for i,v in pairs(game.Workspace.Ignored.Drop:GetChildren()) do
                                            if v.Name == 'MoneyDrop' then
                                                v.BillboardGui.AlwaysOnTop = false
                                                end
                                            end
                                    end
                    
                                    function Start()
                                        getgenv().MoneyEsp = game:GetService("RunService").Heartbeat:Connect(LoopFunction)
                                    end
                                    function Stop()
                                        NoLoop()
                                        getgenv().MoneyEsp:Disconnect()
                                    end

                                    Start()
                                    repeat wait() until moneyesp == false
                                    Stop()
                                end
                            end)                            
                            b:CreateToggle('Players Esp', function(b)
                                PlayersEsp = b 

                                if b then
                                    local esps = {}
                                    function esp(plr)
                                        if getCharacter(plr) and getHead(plr) then
                                            local bbg = Instance.new('BillboardGui')
                                            bbg.Parent = plr.Character
                                            bbg.AlwaysOnTop = true
                                            local textlabel = Instance.new('TextLabel', bbg)
                                            textlabel.Size = UDim2.new(0, 100, 0, 100)
                                            textlabel.TextColor3 = Color3.fromRGB(0, 255, 0)
                                            textlabel.BackgroundTransparency = 1
                                            task.spawn(function()
                                                while wait() do 
                                                    textlabel.Text = plr.Name .. ' / ' .. math.floor((getRoot(speaker).Position - getHead(plr).Position).Magnitude) .. 'm'
                                                end
                                            end)
                                            esps[#esps + 1] = textlabel
                                        end
                                    end

                                    for i,v in pairs(game.Players:GetChildren()) do
                                        if v ~= speaker then
                                            esp(v)
                                            v.CharacterAdded:Connect(function(c)
                                                c:WaitForChild('Head')
                                                if PlayersEsp then
                                                    esp(v)
                                                end
                                            end)
                                        end
                                    end
                                    repeat wait(0) until PlayersEsp == false
                                    for i = 1,#esps do
                                        esps[i]:Destroy()
                                        table.remove(esps, i)
                                    end
                                end
                            end)
