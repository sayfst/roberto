                            local botoes = {}
                            local botoesExec = {}
                            function botoes:CreateButton(title, callback, color)
                                color = color or Color3.fromRGB(255, 255, 255)
                                


                                local Botao = Instance.new("TextButton")



                                Botao.Name = "Botao"
                                Botao.Parent = ScrollingFrame
                                Botao.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                                Botao.BackgroundTransparency = 1.000
                                Botao.Position = UDim2.new(-0.00267379684, 0, 0, 0)
                                Botao.Size = UDim2.new(0, 375, 0, 17)
                                Botao.Font = Enum.Font.RobotoCondensed
                                Botao.TextColor3 = Color3.fromRGB(255, 255, 255)
                                Botao.TextSize = 14.000
                                Botao.Text = title
                                Botao.TextXAlignment = Enum.TextXAlignment.Left
                                getgenv().ExecCmd[#getgenv().ExecCmd + 1] = {
                                    tt = Botao.Text,
                                    ff = callback
                                }
                                Botao.MouseButton1Click:Connect(function()
                                    pcall(callback, Botao)
                                end)
                                local isbind = false
                                if color == 'rainbow' then
                                    task.spawn(function()
                                        while true do
                                            for i = 1,255,10 do
                                                color = Color3.fromRGB(255, i, 0)
                                                task.wait(tempoRGB)
                                            end
                                            for i = 255,0, -10 do
                                                color = Color3.fromRGB(i, 255, 0)
                                                task.wait(tempoRGB)
                                            end
                                            for i = 1, 255,10 do
                                                color = Color3.fromRGB(0, 255, i)
                                                task.wait(tempoRGB)
                                            end
                                            for i = 255,0, -10 do
                                                color = Color3.fromRGB(0, i, 255)
                                                task.wait(tempoRGB)
                                            end
                                            for i = 1, 255,10 do
                                                color = Color3.fromRGB(i, 0, 255)
                                                task.wait(tempoRGB)
                                            end
                                            for i = 255,0, -10 do
                                                color = Color3.fromRGB(255, 0, i)
                                                task.wait(tempoRGB)
                                            end
                                        end
                                    end)
                                end
                                task.spawn(function()
                                    while true do
                                        Botao.TextColor3 = color
                                        wait()
                                    end
                                end)
                            end
                            function botoes:CreateToggle(title, callback)

                                
