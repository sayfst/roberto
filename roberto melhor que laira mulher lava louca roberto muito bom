                                toggleTexto.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                                toggleTexto.BackgroundTransparency = 1.000
                                toggleTexto.BorderSizePixel = 0
                                toggleTexto.Position = UDim2.new(0.0853333324, 0, 0, 0)
