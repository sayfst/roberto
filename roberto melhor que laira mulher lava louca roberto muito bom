                                local toggled = false
                                local warn = false
                                local function force(istoggled)
                                    istoggled = istoggled or 'nottoggled'
                                    toggled = not toggled

                                    if istoggled == 'nottoggled' then
                                        warn = true
                                    else
                                        warn = false
                                    end
                                    if toggled == true then
                                        task.spawn(function()
                                            local step = game.RunService.Heartbeat:Connect(function()
                                                pcall(callback)
                                            end)

                                            task.spawn(function()
                                                repeat wait() until toggled == false
                                                step:Disconnect()
                                                
                                            end)
                                        end)
                                        FrameToggle.Text = ''
                                        FrameToggle.BackgroundColor3 = Color3.fromRGB(203, 145, 203)
                                    else
                                        FrameToggle.BackgroundColor3 = Color3.fromRGB(38, 40, 38)
                                    end
                                end

