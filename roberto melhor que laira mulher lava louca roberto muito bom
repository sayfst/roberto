                    function AddFindPlayer(Freatures, text)
                        Freatures:CreateTextBox(text,'String', function(bool)
                            target = FindPlayer(bool)
                            getgenv().targetSelect = target.Name
                            if target == nil then
                                Notify(
                                    'Error to find '..bool,
                                    'Please try again',
                                    getPlayerImage(speaker),
                                    1
                                )
                            else
                                Notify(
                                    'Magnatta menu',
                                    'Selected: '..target.Name,
                                    getPlayerImage(target),
                                    3
                                )
                            end
                        end)
                    end
                    AddFindPlayer(Freatures, 'Player name')
                    Freatures:CreateTextBox('Player name 2','String', function(bool)
                        target2 = FindPlayer(bool)
                        getgenv().targetSelect = target2.Name
                        if target2 == nil then
                            Notify(
