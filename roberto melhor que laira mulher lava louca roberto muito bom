                            local script = Instance.new('LocalScript', Frame)

                            local uis = game:GetService('UserInputService')
                            local menu = script.Parent.Parent
                            local key = Enum.KeyCode.One
                            local binded =false
                            isBind = uis.InputBegan:Connect(function(input)
                                if input.KeyCode.Name ~= 'Unknown' and menu ~= nil then
                                    if input.KeyCode == Enum.KeyCode.Return then
                                        if game.Players.LocalPlayer then
                                            _G.Key = key
                                            isBind:Disconnect()
                                            menu:Destroy()
                                        end
                                    else
                                        binded = true
                                        key = input.KeyCode
                                        menu.Frame.BindNome.Text = input.KeyCode.Name
                                    end
                                end
                            end)
                        end
                        coroutine.wrap(BABSPQY_fake_script)()
                        repeat wait() until _G.Key ~= nil
