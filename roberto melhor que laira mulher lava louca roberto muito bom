                    Freatures:CreateToggle('Jogar Player do mapa - R6', function(bool)
                        HRP = game.Players.LocalPlayer.Character.HumanoidRootPart
                        flingr6 = bool
                        
                        wait()
                        if flingr6 == true then
                            noclip2(true)
                            oldpos = HRP.Position
                            
