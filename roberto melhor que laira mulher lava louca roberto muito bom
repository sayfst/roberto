                            function drop_esp(v)
                                local text = Drawing.new('Text')
                                text.Visible = false
                                text.Size = 20
                                text.Center = true
                                text.Outline = true
                                text.OutlineColor = Color3.fromRGB(126, 0, 199)
                                text.Font = 3
                                function update()
                                    task.spawn(function()
                                        repeat wait() until configs.Drop_esp == true
                                        local drop = game.RunService.RenderStepped:Connect(function()
                                            local Vector, OnScreen = workspace.CurrentCamera:worldToViewportPoint(v.Briefcase.Position)
                                            if OnScreen then
                                                text.Text = 'Drop [Distance '..math.floor((getRoot(speaker).Position - v.Briefcase.Position).Magnitude) .. 'm]'
                                                text.Position = Vector2.new(Vector.X, Vector.Y)
                                                text.Visible = configs.Drop_esp
                                            else
                                                text.Visible = false
                                            end
                                        end)
                                        workspace.ChildRemoved:Connect(function(b)
                                            if b == v then
                                                text:Remove()
                                                drop:Disconnect()
                                            end
                                        end)
                                    end)                                    
                                end
                                coroutine.wrap(update)()
                            end
                            for i,v in pairs(workspace:GetChildren()) do
                                if v.Name == 'Drop' then
                                    drop_esp(v)
                                end
                            end
                            workspace.ChildAdded:Connect(function(w)
                                if w.Name == 'Drop' then 
                                    drop_esp(w) 
                                end
                            end)
