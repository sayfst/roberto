                            A=Instance.new'Animation'
                            A.Parent=game.Players.LocalPlayer.Character
                            A.Name="AnimationSarrar"
                            A.AnimationId='rbxassetid://148840371'
                            local P=game:GetService'Players'.LocalPlayer
                            local C=P.Character or P.CharacterAdded:Wait()
                            H=C:WaitForChild'Humanoid':LoadAnimation(A)
                            IsPlay = true
                            H:Play()
                            H:AdjustSpeed(2.5)
                            if game.Players.LocalPlayer.Character.Humanoid.Health == 0 then
                                wait(0.3)
                                ative()
                            end
                            spawn(function()
                                repeat wait() until speaker.Character:WaitForChild('Humanoid').Died:wait() or IsPlay == false
    
                                if IsPlay ~= false then
                                    wait(0.3)
                                    ative()
                                end
                                
                            end)
                        else
                            IsPlay = false
                            H:Stop()
                        end
                    end)
                    
                    local uis = game:GetService('UserInputService')
                    local canNew = true
                    a = uis.InputBegan:Connect(function(input)
                        if input.KeyCode == Enum.KeyCode.RightControl and canNew == true then
                            canNew = false
                            b = uis.InputBegan:Connect(function(input)
                                if input.UserInputType == Enum.UserInputType.MouseButton1 then
                                    b:Disconnect()
                                    local mouse = speaker:GetMouse()
                                    local pos = mouse.Hit.Position + Vector3.new(0, 2.5, 0)
                                    local oldTg = target
                                    local dist = math.huge
                                    for i,v in pairs(game.Players:GetChildren()) do
                                        if v ~= speaker then
                                            if getRoot(v) then
                                                local mag = (getRoot(v).Position - pos).Magnitude
                                                if mag < dist then
                                                    dist = mag
                                                    target = v
                                                end
                                            end
                                        end
                                    end
                                    canNew = true
                                    if oldTg ~= target then
                                        Notify('Player Select',target.Name,getPlayerImage(target.Name), 5)
                                    else
                                        Notify('Player Select','Error to select',getPlayerImage(speaker), 5)
                                    end
                                    
                                end
                            end)
                        end
                    end)
                    Server:CreateButton('Re-Bind Menu', function()
                        NotifyV2('RE BIND MENU', 'Press any key', 2)
                        local Bind = game.UserInputService.InputBegan:Connect(function(a,b)
                            if b then
                                return 
                            end

                            if a.KeyCode.Name == 'Unknown' then
                                _G.Key = a.UserInputType
                                NotifyV2('Sucess Bind', 'New Bind: '.._G.Key.Name, 1)
                            else
                                _G.Key = a.KeyCode
                                NotifyV2('Sucess Bind', 'New Bind: '.._G.Key.Name, 1)
                            end
                        
                        end)
                        local oldkey = _G.Key
                        repeat wait() until oldkey ~= _G.Key
                        Bind:Disconnect()
                    end)
                    Server:CreateButton('Desinject', function()
                        for i,v in pairs(game:GetService('CoreGui'):GetDescendants()) do
                            if v.Name == 'Magnata v2' or v.Name == 'Magnatta v2 2' then
                                v:Destroy()
                            end
                        end
                    end, Color3.fromRGB(255, 0, 0))


