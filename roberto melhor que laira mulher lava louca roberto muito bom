                            function botoes:CreateTextBox(text, string, callback)
                                string = string or 'String'
                                -- Version: 3.2

                                -- Instances:

                                local TextLabel = Instance.new("TextBox")

                                --Properties:

                                TextLabel.Parent = ScrollingFrame
                                TextLabel.Active = true
                                TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                                TextLabel.BackgroundTransparency = 1.000
                                TextLabel.Position = UDim2.new(-0.00267379684, 0, 0, 0)
                                TextLabel.Selectable = true
                                TextLabel.Size = UDim2.new(0, 375, 0, 17)
                                TextLabel.Font = Enum.Font.RobotoCondensed
                                TextLabel.Text = text
                                TextLabel.TextColor3 = Color3.fromRGB(199, 199, 199)
                                TextLabel.TextSize = 20.000
                                TextLabel.TextXAlignment = Enum.TextXAlignment.Left
                                TextLabel.FocusLost:Connect(function(a)
                                    if a then
                                        if string == 'String' then
                                            pcall(callback, TextLabel.Text)
                                        else
                                            pcall(callback, tonumber(TextLabel.Text))
                                        end
                                    end
                                end)
                            end                            
                            function botoes:CreateWhile(corno, callback)
