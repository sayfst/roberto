                            function getCamaro()
                                local camaro = nil
                                local pos = 1500

                                for i,v in pairs(game:GetService("Workspace").Vehicles:GetChildren()) do
                                    if table.find(carros, v.Name) and v:IsA('Model') and v:FindFirstChild('Seat') and v:FindFirstChild('Engine') then
                                        if v.Seat.Player.Value == false then
                                            local mag = (v.Seat.Position - getRoot(speaker).Position).Magnitude

                                            if mag < pos then
                                                pos = mag
                                                camaro = v
                                            end
                                            
                                        end
                                    end
                                end
                                return camaro
                            end

