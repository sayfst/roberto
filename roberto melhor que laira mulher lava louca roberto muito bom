                                local PlayersToKill = {}
                                local canChange = false
                                for i,v in pairs(game.Players:GetChildren()) do
                                    if getRoot(v) and getRoot(target) then
                                        if v ~= target and
                                        v ~= speaker and
                                        v.Character ~= nil and
                                        v.Character.Humanoid.Health > 0 and
                                        (getRoot(v).Position - getRoot(target).Position).magnitude <=20 and 
                                        not CheckPlayerIsWhitelist(v) then
                                            if v.TeamColor == speaker.TeamColor then
                                                canChange = true
                                            end
                                            PlayersToKill[#PlayersToKill + 1] = v
                                        end
                                    end
                                end
    
