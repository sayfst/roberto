                    local FE = Menu:CreateTab('FE', Color3.fromRGB(255, 0, 0))
    
                    local Binds = Menu:CreateTab('Binds', Color3.fromRGB(255, 0, 0))
    
                    local Binds_Enabled = {

                    }
                    game:GetService('UserInputService').InputBegan:Connect(function(input, ischat)
                        if ischat then return end
                        if input.KeyCode.Name ~= 'Unknown' then
                            for i = 1,#Binds_Enabled do
                                if string.find(input.KeyCode.Name, Binds_Enabled[i][1]) then
                                    ExecCmd(Binds_Enabled[i][2])
                                    ToggleToggle(Binds_Enabled[i][2])
                                     
                                end
                            end
                        end
                    end)
                    Binds:CreateTextBox('Add Bind', 'String', function(text)
                        if string.find(text, '"') then
                            local args = text:split('"')

                            Binds_Enabled[#Binds_Enabled + 1] = {
                                args[1],
                                args[2]
                            }
                            Binds:CreateButton(args[2]..' KeyCode: '..args[1], function(button)
                                button:Destroy()
                                local arg = args[2]
                                for i = 1,#Binds_Enabled do
                                    if Binds_Enabled[i][2] == arg then
                                        table.remove(Binds_Enabled, i)
                                    end
                                end
                            end)
                        end
                    end)
                    
                    Binds:CreateButton('Set Clip Board Tutorial', function()
                        setclipboard('Tutorial, Para bindar um botao, Digite acima: W (Sua tecla) (Coloque um barra e escreva o nome do botao), exemplo: H/Noclip')
                    end)
                    local fileName = 'Laira Binds.lua'
                    Binds:CreateButton('Save Binds', function()
                        writefile(fileName, game:GetService('HttpService'):JSONEncode(Binds_Enabled))
                        Notify(
                            'Save Binds',
                            'Binds salvas com sucesso como '..fileName,
                            nil,
                            3
                        )
                    end)
                    Binds:CreateButton('Load Binds', function()
                        if isfile(fileName) then
                            local file = game.HttpService:JSONDecode(readfile(fileName))

                            Binds_Enabled = file
                            Notify(
                                'Load Binds',
                                'Binds pegas com sucesso como '..fileName,
                                nil,
                                3
                            )
                        end
                    end)
                    

                    local corlegal = Color3.fromRGB(138, 222, 232)
                    local multiList = {}
                    function CheckPlayerIsWhitelist(plr)
                        return false
                    end


                        
                    function addOnWhitelist(plr)
                        multiList[#multiList + 1] = plr.Name
                    end
                    function remOnWhitelist(plr)
                        lugar = 0
                        for i = 1,#multiList do
                            if multiList[i] == plr.Name then
                                table.remove(multiList, i)
                            end 
                        end
    
                    end
                    



    
                    hNeed = ' {Hats needed}'
                    noHneed = ' {No Hats neeeded}'
    
                    function copyHats(hats)
                        local games = hats
                        text = ''
                        num = 0
                        for i,v in pairs(games) do
                            if v then
                                num = num + 1
                                text = text .. '\nHat '..num..': '..v
                            end
                        end
                        setclipboard(text)
                    end
                    function R15(speaker)
                        if speaker.Character.Humanoid.RigType == Enum.HumanoidRigType.R15 then
                            return true;
                        else
                            return false;
                        end
                    end

                    FE:CreateButton('{R6} - Slenderman FE'..hNeed, function()
                        if not R15(speaker) then
                            if FindAccessory('Nagamaki') and FindAccessory('MeshPartAccessory') then
                                pcall(function() local Hats_Need = { ''; ''; } local speaker = game.Players.LocalPlayer _G.On = false if speaker.Character ~= nil and speaker.Character:FindFirstChild('MeshPartAccessory') ~= nil and speaker.Character:FindFirstChild('Nagamaki') ~= nil then local Faca = speaker.Character:FindFirstChild('Nagamaki') local Cabeca = speaker.Character:FindFirstChild('MeshPartAccessory') local Head = speaker.Character.Head local Right = speaker.Character['Right Arm'] local Left = speaker.Character['Left Arm'] function RemWelds() pcall(function() Faca.Handle.AccessoryWeld:Destroy() Cabeca.Handle.AccessoryWeld:Destroy() end); end function HipHeight() speaker.Character.Humanoid.HipHeight = 2 end function BlockHead() game.Players.LocalPlayer.Character.Head:FindFirstChildOfClass("SpecialMesh"):Destroy() game.Players.LocalPlayer.Character.Head.face:Destroy() end function RemVelocity() Faca.Handle.Velocity = Vector3.new(0, 0, 0) end function RemBracos() speaker.Character.Torso['Left Shoulder']:Destroy() speaker.Character.Torso['Right Shoulder']:Destroy() end RemWelds() BlockHead() HipHeight() RemBracos() spawn(function() while task.wait() do Right.CFrame = speaker.Character['Right Leg'].CFrame * CFrame.new(0, -1.8, 0) Left.CFrame = speaker.Character['Left Leg'].CFrame * CFrame.new(0, -1.8, 0) Right.Velocity = Vector3.new(0, 0, 0) Left.Velocity = Vector3.new(0, 0, 0) end end) spawn(function() while task.wait() do Cabeca.Handle.CFrame = Head.CFrame * CFrame.new(0, 1, 0) Cabeca.Handle.Orientation = Cabeca.Handle.Orientation + Vector3.new(0, -90, 0) end end) function GuardarFaca() _G.On = false delay(.1, function() _G.On = true speaker.Character.Humanoid.WalkSpeed = 16 while _G.On == true do Faca.Handle.CFrame = speaker.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 0.6) task.wait() end end) end local POS = 0.7 function EquiparFaca() _G.On = false delay(0.1, function() _G.On = true away = 0 reverse = false while _G.On == true do speaker.Character.Humanoid.WalkSpeed = 23 if reverse == false then away = away + 0.01 else away = away - 0.01 end if away >=0.5 then reverse = true elseif away < -0.4 then reverse = false end Faca.Handle.CFrame = speaker.Character.Torso.CFrame * CFrame.new(1.8, away, -POS) Faca.Handle.Orientation = Faca.Handle.Orientation + Vector3.new(0, 50, 0) task.wait() end end) end EquiparFaca() local Equipped = false game:GetService('StarterGui'):SetCore('SendNotification', { Title = 'Sucess loaded FE slenderman'; Text = 'Binds:\n "F" = Equip/Unequip KNIFE\n"G" = Punch' }) speaker:GetMouse().KeyDown:Connect(function(Key) if Key:lower() == 'f' then Equipped = not Equipped if Equipped == true then GuardarFaca() else EquiparFaca() end elseif Key:lower() == 'g' then if Equipped == true then pcall(function() speaker.Character.HumanoidRootPart.Velocity = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.lookVector * 50 end); spawn(function() local part = Instance.new("Part", game.Players.LocalPlayer.Character) part.Transparency = 0 part.Size = Vector3.new(5, 2, 3) part.CanCollide = false local w1 = Instance.new("Weld", part) w1.Part0 = game.Players.LocalPlayer.Character.Torso w1.Part1 = part w1.C1 = CFrame.new(0,0,2) mainRemotes = game.ReplicatedStorage meleeRemote = mainRemotes['meleeEvent'] part.Touched:connect(function(hit) if game.Players:FindFirstChild(hit.Parent.Name) then local plr = game.Players:FindFirstChild(hit.Parent.Name) if plr.Name ~= game.Players.LocalPlayer.Name then part:Destroy() for i = 1,100 do meleeRemote:FireServer(plr) end end end end) end) end end end) end end)
                            else
                                NotifyCallback('SlenderMan FE Error', 'Error finding the necessary accessories', nil, 5, 'Copy Link', 'Ok', function()
                                    setclipboard('Hat 1: https://www.roblox.com/catalog/7390040662/Power-25\nHat 2: https://www.roblox.com/catalog/4855525473/Nagamaki\nTotal Robux needed: 125')
                                end, function()end)
                            end
                        else
                            Notify('Error', 'The script is for R6')
                        end
                    end)                
