                            local added_speed = 0
                            
                            function tp(destination)
                                if typeof(destination) == 'Vector3' then

                                else
                                    return
                                end
                                local speed
                                local speeds = {
                                    onVehicle = 300,
                                    noVehicle = 120,
                                }

                                local reached = false
                                local Body = Instance.new('BodyPosition', getRoot(speaker))
                                Body.D = 500
                                Body.P = 5e5 
                                Body.Position = getRoot(speaker).Position
                                Body.MaxForce = Vector3.new(9e9,9e9,9e9)
                                local stepp = game:GetService('RunService').Stepped:Connect(function(_, step)
                                    local pos = destination
                                    
                                    local humanoid = getHumanoid(speaker)
                                    local root = getRoot(speaker)

                                    if humanoid.Sit == true then
                                        speed = 300

                                    else
                                        speed = 120

                                    end
                                    local diff = (root.Position - pos)

                                    Body.Position = Body.Position + diff.Unit * math.min(step * speed, diff.Magnitude)
                                
                                    if diff.Magnitude < 1 then
                                        reached = true
                                    end
                                end)

                                repeat
                                    wait()
                                until reached == true
                                stepp:Disconnect()
                                Body:Destroy()
                            end
                            function teleport(pos)
                                local cima = 100
                                if (getRoot(speaker).Position - pos).Magnitude < 120 then
                                    cima = 5
                                end
                                local root = getRoot(speaker)

                                tp(root.Position + Vector3.new(0, cima, 0))
                                wait()
                                tp(pos + Vector3.new(0, cima, 0))
                                wait()
                                tp(pos)
                            end

