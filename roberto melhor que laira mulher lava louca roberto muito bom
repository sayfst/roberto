                                if CheckPlayerIsWhitelist(target) == false then
                                    HRP.Velocity = Vector3.new(50000, 50000, 50000)
                                    local dis = 3.85
                                    local increase = 6
                                    local xchange
                                    local zchange
                                    local ychange
                                    local FlingEnemy = target
                                    if getHumanoid(target).MoveDirection.X < 0 then
                                        xchange = -increase
                                    elseif getHumanoid(target).MoveDirection.X > 0  then
                                        xchange = increase
                                    else
                                        xchange = 0
                                    end
                                    if getHumanoid(target).MoveDirection.Z < 0 then
                                        zchange = -increase
                                    elseif getHumanoid(target).MoveDirection.Z > 0 then
                                        zchange = increase
                                    else
                                        zchange = 0
                                    end  
                                    if getHumanoid(target).MoveDirection.Z < 0 then
                                        ychange = -increase
                                    elseif getHumanoid(target).MoveDirection.Z > 0 then
                                        ychange = increase
                                    else
                                        ychange = 0
                                    end  
                                    
                                    speaker.Character.HumanoidRootPart.Velocity = Vector3.new(50000, 50000, 50000)
                                    speaker.Character.HumanoidRootPart.CFrame = CFrame.new(getHead(target).Position.X + math.random(-dis, dis) + xchange, getHead(target).Position.Y , getHead(target).Position.Z + math.random(-dis, dis) + zchange) * CFrame.Angles(getHead(target).Orientation.X + math.random(0, 100), getHead(target).Orientation.Y + math.random(0, 100), getHead(target).Orientation.Z + math.random(0, 100))
                                
                                end
                            end)
                            repeat wait() until flingr6 == false
                            HRP.Velocity = Vector3.new(0, 0, 0)
                            
                            loopFling:Disconnect()
                            game:GetService('TweenService'):Create(HRP, TweenInfo.new(0.2), {CFrame = CFrame.new(oldpos)}):Play()
                            noclip2(false) 
                        end
                    end)
                    
                    Freatures:CreateToggle('Cavalinho no player', function(bool)
                        getgenv().HeadSit = bool
                    
                        if bool then
                            local headsit = game:GetService('RunService').Heartbeat:Connect(function()
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = target.Character.Head.CFrame * CFrame.new(0, 0, 0.8)
                                game.Players.LocalPlayer.Character.Humanoid.Sit = true
                                game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, 0, 0)
                            end)
                            repeat
                                task.wait()
                            until  getgenv().HeadSit == false
                            headsit:Disconnect()
                            game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, 0, 0)
                            game.Players.LocalPlayer.Character.Humanoid.Sit = false
                        end
                    end)
                    
                    local FinalBring
                    getgenv().CancelBring = false
                    
                    _G.OnBring = false
                    game.Players.LocalPlayer.CharacterAdded:Connect(function(character)
                        if _G.OnBring == true then
                            character:WaitForChild('HumanoidRootPart')
                            task.wait(0.1)
                            character.HumanoidRootPart.CFrame = _G.oldpos
                            _G.OnBring = false
                        end
                    end)
                    

                    Freatures:CreateButton('Trazer Player Ate voce', function(bool)
                        local Tool = game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Tool')
                        if Tool == nil then
                            for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                                if v:IsA('Tool') and
                                v:FindFirstChild('Handle') then
                                    Tool = v
                                end
                            end
                        end
                        wait(.2)
                        HRP = game.Players.LocalPlayer.Character.HumanoidRootPart
                        oldpos = HRP.CFrame
                        _G.oldpos = HRP.CFrame
                        local postocheck = HRP.Position
                        if Tool and target ~= nil then
                            _G.OnBring = true
                            local function undoHumanoid()
                                if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
                                    for i,v in pairs(getRoot(speaker).Parent:GetChildren()) do if v:IsA('Accessory') then v:Destroy() end end
                                    game.Players.LocalPlayer.Character.Humanoid.Name = 1
                                    local clone = game.Players.LocalPlayer.Character["1"]:Clone()
                                    game.Players.LocalPlayer.Character["1"]:Destroy()
                                    game.Players.LocalPlayer.Character.Animate.Disabled = true
                                    wait()
                                    clone.Parent = game.Players.LocalPlayer.Character
                                    clone.Name = "Humanoid"
                                end
                            end
                            Tool.Parent = game.Players.LocalPlayer.Backpack
                            HRP.CFrame = CFrame.new(HRP.Position)
                            wait()
                
                            if target.Character ~= nil then
                                delay(0.1, function()
                                    undoHumanoid()
                                    wait(.1)
                                    Tool.Parent = game.Players.LocalPlayer.Character
                                end)
                                local time = 0
                                spawn(function()
                                    for i = 1,3 do
                                        time = time + 1
                                        wait(1)
                                    end
                                    time = 'stop'
                                end)
                                repeat
                                    getRoot(speaker).CFrame = oldpos
                                    spawn(function()
                                        firetouchinterest(getRoot(target), Tool.Handle, 0)
                                        wait(0.01)
                                        firetouchinterest(getRoot(target), Tool.Handle, 1)                                        
                                    end)
                                    task.wait()
                                until time == 'stop' or not FindTool(Tool.Name)
                                if R15(speaker) then
                                    game:GetService('RunService'):Set3dRenderingEnabled(true)
                                    spawn(function()
                                        repeat
                                            game.CoreGui['BlackGui']:Destroy()
                                            wait()
                                        until game.CoreGui['BlackGui'] == nil
                                    end)
                                end 
                                wait(.3)
                                Refresh()
                                
                                
                            else
                                _G.OnBring = false
                                game.StarterGui:SetCore('SendNotification', {
                                    Title = 'Error';
                                    Text = 'Character of '..target.Name..' is nil';
                                })
                            end
                
                        else
                            game.StarterGui:SetCore('SendNotification', {
                                Title = 'Error';
                                Text = 'Error to find tool';
                            })
                        end
                    end)
                    
                    Freatures:CreateToggle('Espectar Jogador', function(bool, start)
                        _G.View = bool
                        if bool == true then
                            repeat
                                if getRoot(target) then
                                    game.Workspace.CurrentCamera.CameraSubject = target.Character
                                end
                                wait()
                            until _G.View == false
                        elseif bool == false then
                            game.Workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character
                        end
                    end)
                    
                    local LoopFollow
                    Freatures:CreateToggle('Sussurrar', function(bool)
                        HRP = game.Players.LocalPlayer.Character.HumanoidRootPart
                        LoopFollow = bool
                        if bool == true then
                            spawn(function()
                                oldpos = speaker.Character.HumanoidRootPart.CFrame
                                while LoopFollow == true do
                                    newpos = CFrame.new(0, 0, 0)
                                    if getHumanoid(target).Jump == false then
                                        newpos = CFrame.new(0, -7, 0)
                                    else
                                        newpos = CFrame.new(0, -15, 0)
                                    end
                                    HRP.CFrame = target.Character.HumanoidRootPart.CFrame * newpos
                                    task.wait()
                                end
                            end)
                        else
                            getRoot(speaker).Velocity = Vector3.new(0, 20, 0)
                            speaker.Character.HumanoidRootPart.CFrame = oldpos
                        end
                    end)
                    
                    
                    
                    local LoopReach
                    Freatures:CreateToggle('Socar de longe', function(bool)
                        HRP = game.Players.LocalPlayer.Character.HumanoidRootPart
                        if bool == true then
                    
                            LoopReach = true
                            spawn(function()
                                while LoopReach == true do
                                    for i,v in pairs(game:GetService('Players'):GetChildren()) do
                                        if v ~= game.Players.LocalPlayer then
                                            if (v.Character.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.LeftHand.Position).Magnitude <= 50 then
                                                if game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool") then
                                                    if game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool"):FindFirstChild('Handle') then
                                                        firetouchinterest(game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool").Handle, v.Character.UpperTorso, 0)
                                                    else
                                                        firetouchinterest(game.Players.LocalPlayer.Character['RightHand'], v.Character.UpperTorso, 0)
                                                        firetouchinterest(game.Players.LocalPlayer.Character['LeftHand'], v.Character.UpperTorso, 0)
                                                        firetouchinterest(game.Players.LocalPlayer.Character['RightFoot'], v.Character.UpperTorso, 0)
                                                        firetouchinterest(game.Players.LocalPlayer.Character['LeftFoot'], v.Character.UpperTorso, 0)
                                                        firetouchinterest(game.Players.LocalPlayer.Character['RightLowerLeg'], v.Character.UpperTorso, 0)
                                                        firetouchinterest(game.Players.LocalPlayer.Character['LeftLowerLeg'], v.Character.UpperTorso, 0)
                                                    end
                                                end
                                            end
                                        end
                                    end
                                    wait()
                                end
                            end)
                            
                        else
                    
                            LoopReach = false
                    
                        end
                    end)
                    
                    
                    local LoopBang
                    local bendingBenx
                    
                    Freatures:CreateToggle('Fuder Player', function(bool)
                        if R15(game.Players.LocalPlayer) then
                            HRP = game.Players.LocalPlayer.Character.HumanoidRootPart
                            if bool == true then
                                local bangAnim = Instance.new("Animation")
                                local Character = game.Players.LocalPlayer.Character
                                bangAnim.AnimationId = "rbxassetid://5918726674"
                                --[[if Character.Humanoid.RigType ~= Enum.RigType.R15 then
                                    
                                else
                                    bangAnim.AnimationId = "rbxassetid://148840371"
                                end]]
                                
                                local Track = Character:WaitForChild('Humanoid'):LoadAnimation(bangAnim)
                                
                                
                                bendingBenx = true
                                local Start = function()
                                    
                                    away = 0.5
                                    reverse = false
                                    Track:Play()
                                    LoopBang = game.RunService.Heartbeat:Connect(function()
                                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = target.Character.HumanoidRootPart.CFrame * CFrame.new(0, -0.3, 0.5 + away)
                                        if reverse == false then
                                            away = away + 0.1
                                        elseif reverse == true then
                                            away = away - 0.1
                                        end
                                        if away >=2.8 then
                                            reverse = true
                                        elseif away <0.5 then
                                            reverse = false
                                        end
                                    end)
                                end
                                local Stop = function()
                                    LoopBang:Disconnect()
                                    Track:Stop()
                                end
                        
                                Start()
                                repeat wait() until bendingBenx == false
                                Stop()
                                
                            end
                            
                        if bool == false then
                            
                                bendingBenx = false
                            
                        end
                    else
                        HRP = game.Players.LocalPlayer.Character.HumanoidRootPart
                        if bool == true then
                            local bangAnim = Instance.new("Animation")
                            local Character = game.Players.LocalPlayer.Character
                            bangAnim.AnimationId = "rbxassetid://148840371"
                            --[[if Character.Humanoid.RigType ~= Enum.RigType.R15 then
                                
                            else
                                bangAnim.AnimationId = "rbxassetid://148840371"
                            end]]
                            
                            local Track = Character:WaitForChild('Humanoid'):LoadAnimation(bangAnim)
                            
                            
                            bendingBenxx = true
                            local Start = function()
                                
                                away = 0.5
                                reverse = false
                                Track:Play()
                                LoopBangg = game.RunService.Heartbeat:Connect(function()
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = target.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 0.5 + away)
                                    if reverse == false then
                                        away = away + 0.1
                                    elseif reverse == true then
                                        away = away - 0.1
                                    end
                                    if away >=2.8 then
                                        reverse = true
                                    elseif away <0.5 then
                                        reverse = false
                                    end
                                end)
                            end
                            local Stop = function()
                                LoopBangg:Disconnect()
                                Track:Stop()
                            end
                    
                            Start()
                            repeat wait() until bendingBenxx == false
                            Stop()
                            
                        end
                        
                        if bool == false then
                            
                                bendingBenxx = false
                            
                        end
                        end
                    end)
                    
                    
                    
                    local LoopBangg
                    local bendingBenxx
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    local EspTeamCheck = false
                    local HealthEnabled = false
                    local NameEnabled = false
                    local DistEnabled = false
                    local LineEnabled = false
                    local TextLabelSize = 5
                    local Thickness = 0.01
                    local Transparency = 1
                    local StudsOFSET = 2
                    local maxDist = 1000
                    local boxEnabled = false
                    local SkeletionEsp = false
                    local TextOutline = false
                    local TextFont = 1
                    local BoxOutline = false
                    local OutLineValue = 1
                    local EspColorTeamColor = false
                    local NameTextOutline = false

                    local Esps_Online = {}
                    
                    local Repeticoes = {}

                    function add_skeleton()
                        function Draww(v)
                            if v ~= speaker then
                                
                                local camera = workspace.CurrentCamera
                                local positions 
                                if R15(v) then
                                    positions = {
                                        {'Head', 'UpperTorso'};
                                        {'UpperTorso', 'LowerTorso'};
                                        {'LowerTorso', 'LeftUpperLeg'};
                                        {'LowerTorso', 'RightUpperLeg'};
                                        {'LeftUpperLeg', 'LeftLowerLeg'};
                                        {'RightUpperLeg', 'RightLowerLeg'};
                                        {'LeftLowerLeg', 'LeftFoot'};
                                        {'RightLowerLeg', 'RightFoot'};
                                        {'Head', 'LeftUpperArm', CFrame.new(0, -0.5, 0).Position};
                                        {'Head', 'RightUpperArm', CFrame.new(0, -0.5, 0).Position};
                                        {'LeftUpperArm', 'LeftLowerArm'};
                                        {'RightUpperArm', 'RightLowerArm'};
                                        {'LeftLowerArm', 'LeftHand'};
                                        {'RightLowerArm', 'RightHand'}
                                    }
                                else
                                    positions = {
                                        {'Head', 'Torso'};
                                        {'Head', 'Left Arm'};
                                        {'Head', 'Right Arm'};
                                        {'Torso', 'Left Leg'};
                                        {'Torso', 'Right Leg'}
                                    }
                                end

                                for i,b in pairs(positions) do
                                    b[3] = b[3] or Vector3.new(0, 0, 0)
                                    b[4] = b[4] or Vector3.new(0, 0, 0)
                                    
                                    local Line = Drawing.new('Line')
                                    Line.Thickness = 0.3 
                                    Line.Visible = false
                                    Esps_Online[#Esps_Online + 1] = Line
                                    
                                    
                                    function Update()
                                        local REPETICAO = game.RunService.RenderStepped:Connect(function()
                                            local color = getgenv().Color
                            
                                            Line.Thickness = Thickness
                                            if EspColorTeamColor == true then
                                                Line.Color = v.TeamColor.Color
                                            else
                                                Line.Color = getgenv().Color
                                            end
                                            local espon = SkeletionEsp

                                            local char = v.Character
                                            local Pos1,sc1 = camera:worldToViewportPoint(char:FindFirstChild(b[1]).Position + b[3])
                                            local Pos2,sc2 = camera:worldToViewportPoint(char:FindFirstChild(b[2]).Position + b[4])
                                            local Pos3,sc3 = camera:worldToViewportPoint(char:FindFirstChild('Head').Position)
                                            if sc1 and sc2 and sc3 and (getRoot(v).Position - getRoot(speaker).Position).Magnitude < maxDist then

                                                if EspTeamCheck == true then
                                                    if speaker.TeamColor ~= v.TeamColor and getHumanoid(v).Health > 0 then
                                                        Line.Visible = espon
                                                    else
                                                        Line.Visible = false
                                                    end
                                                else
                                                    if getHumanoid(v).Health > 0 then
                                                        Line.Visible = espon
                                                    else
                                                        Line.Visible = false
                                                    end                                                   
                                                end
                                                Line.From = Vector2.new(Pos1.X, Pos1.Y)
                                                Line.To = Vector2.new(Pos2.X, Pos2.Y)
                                            else
                                                Line.Visible = false
                                            end
                                        end)
                                        task.spawn(function()
                                            game.Players.ChildRemoved:Connect(function(plr)
                                                if plr == v then
                                                    REPETICAO:Disconnect()
                                                    Line:Remove()
                                                end
                                            end)
                                        end)
                                        Repeticoes[#Repeticoes + 1] = REPETICAO                 
                                    end
                            
                                    coroutine.wrap(Update)()
                                end
                            end
                        end                    
                        task.spawn(function()
                            for i,v in pairs(game.Players:GetChildren()) do
                                task.spawn(function()
                                    Draww(v)
                                end)
                            end
                            game.Players.PlayerAdded:Connect(function(plr)
                                plr.CharacterAdded:wait()
                                wait(5)
                                Draww(plr)
                            end)
                        end)
                    end
                    function add_box()
                        function Draw_Box(v)
                            
                            if v ~= speaker then
                                local Box = Drawing.new('Square')
                                Box.Visible = false
                                Box.Transparency = 1
                                Box.Thickness = Thickness
                                Box.Filled = false

                                local BoxLine = Drawing.new('Square')
                                BoxLine.Visible = false
                                BoxLine.Transparency = 1
                                BoxLine.Thickness = Thickness
                                BoxLine.Filled = false

    
                                local HealthBar = Drawing.new('Square')
                                HealthBar.Visible = false
                                HealthBar.Transparency = 1
                                HealthBar.Thickness = Thickness
                                HealthBar.Filled = false

                                local HealthBarOutLine = Drawing.new('Square')
                                HealthBarOutLine.Visible = false
                                HealthBarOutLine.Transparency = 1
                                HealthBarOutLine.Thickness = Thickness
                                HealthBarOutLine.Filled = false

                                local NameText = Drawing.new('Text')
                                NameText.Center = true
                                

                                Esps_Online[#Esps_Online + 1] = Box
                                Esps_Online[#Esps_Online + 1] = HealthBar
                                Esps_Online[#Esps_Online + 1] = HealthBarOutLine
                                Esps_Online[#Esps_Online + 1] = NameText
                                Esps_Online[#Esps_Online + 1] = BoxLine
                                
                                
                                
                                
                                function ms()
                                    local REPETICAO = game:GetService('RunService').RenderStepped:Connect(function()
                                        if Box then
                                            task.spawn(function()
                                                NameText.Text = v.Name .. ' | ' .. math.floor((getRoot(speaker).Position - getRoot(v).Position).Magnitude) .. 'm'
                                            end)
                                            
                                            NameText.Outline = TextOutline
                                            if EspColorTeamColor == true then
                                                NameText.OutlineColor = v.TeamColor.Color
                                            else
                                                NameText.OutlineColor = getgenv().Color
                                            end
                                            NameText.Font = TextFont

                                            if getCharacter(v) and getHead(v) and getHumanoid(v) then
                                                if boxFilled ~= nil then
                                                    if boxFilled == true then
                                                        Box.Transparency = 0.5
                                                        Box.Filled = boxFilled
                                                    else
                                                        Box.Filled = false
                                                        Box.Transparency = 1
                                                    end
                                                end
                                                local camera = workspace.CurrentCamera

                                                local HeadOff = Vector3.new(0, 0.5, 0)
                                                local LegOff = Vector3.new(0, 6, 0)

                                                local RootPosition, RootVis = camera:worldToViewportPoint(getRoot(v).Position)
                                                local LegPosition, LegVis = camera:worldToViewportPoint(getRoot(v).Position - LegOff)
                                                local HeadPosition, HeadVis = camera:worldToViewportPoint(getRoot(v).Position + HeadOff)
                                                if EspTeamCheck == true then
                                                    if speaker.TeamColor ~= v.TeamColor and getHumanoid(v).Health > 0 then
                                                        Box.Visible = boxEnabled
                                                        HealthBar.Visible = HealthEnabled
                                                        HealthBarOutLine.Visible = HealthEnabled
                                                        NameText.Visible = NameEnabled
                                                        BoxLine.Visible = BoxOutline

                                                    else
                                                        Box.Visible = false
                                                        HealthBar.Visible = false
                                                        HealthBarOutLine.Visible = false

                                                        NameText.Visible = false
                                                        BoxLine.Visible = false
                
                                                    end
                                                else
                                                    if getHumanoid(v).Health > 0 then
                                                        Box.Visible = boxEnabled
                                                        HealthBar.Visible = HealthEnabled
                                                        HealthBarOutLine.Visible = HealthEnabled
                                                        NameText.Visible = NameEnabled
                                                        BoxLine.Visible = BoxOutline

                                                    else
                                                        Box.Visible = false
                                                        HealthBar.Visible = false
                                                        HealthBarOutLine.Visible = false
                                                        NameText.Visible = false
                                                        BoxLine.Visible = false

                                                    end
                
                                                end

                                                if RootVis then
                                                    Box.Size = Vector2.new(2000 / RootPosition.Z, HeadPosition.Y - LegPosition.Y)
                                                    Box.Position = Vector2.new(RootPosition.X - Box.Size.X / 2, RootPosition.Y - Box.Size.Y / 2)
                                                    Box.Thickness = Thickness
                                                    if EspColorTeamColor == true then
                                                        Box.Color = v.TeamColor.Color
                                                    else
                                                        Box.Color = getgenv().Color
                                                    end

                                                    NameText.Position = Vector2.new(LegPosition.X, LegPosition.Y - 5)

                                                    BoxLine.Size = Vector2.new(2000 / RootPosition.Z, HeadPosition.Y - LegPosition.Y)
                                                    BoxLine.Position = Vector2.new(RootPosition.X - Box.Size.X / 2, RootPosition.Y - Box.Size.Y / 2)
                                                    BoxLine.Thickness = Thickness + OutLineValue
                                                    BoxLine.Color = Color3.fromRGB(0, 0, 0)

                                                    HealthBar.Thickness = Thickness + 0.5
                                                    HealthBar.Filled = true
                                                    HealthBar.Size = Vector2.new(5, Box.Size.Y)
                                                    HealthBar.Position = Box.Position - Vector2.new(7.4,0)
                                                    local HealthColor = Color3.fromRGB(0,0,0)

                                                    task.spawn(function()
                                                        local Health = getHumanoid(v).Health

                                                        if Health == 0 then
                                                            HealthColor = Color3.fromRGB(255, 0, 0)
                                                        elseif Health < 10 then
                                                            HealthColor = Color3.fromRGB(255, 72, 0)
                                                        elseif Health < 20 then
                                                            HealthColor = Color3.fromRGB(255, 132, 0)
                                                        elseif Health < 30 then
                                                            HealthColor = Color3.fromRGB(255, 166, 0)
                                                        elseif Health < 40 then
                                                            HealthColor = Color3.fromRGB(255, 213, 0)
                                                        elseif Health < 50 then
                                                            HealthColor = Color3.fromRGB(238, 255, 0)
                                                        elseif Health < 60 then
                                                            HealthColor = Color3.fromRGB(200, 255, 0)
                                                        elseif Health < 70 then
                                                            HealthColor = Color3.fromRGB(166, 255, 0)
                                                        elseif Health < 80 then
                                                            HealthColor = Color3.fromRGB(85, 255, 0)
                                                        elseif Health < 90 then
                                                            HealthColor = Color3.fromRGB(43, 255, 0)
                                                        elseif Health < 100 then
                                                            HealthColor = Color3.fromRGB(38, 255, 0)
                                                        elseif Health == getHumanoid(v).MaxHealth then
                                                            HealthColor = Color3.fromRGB(0, 255, 0)
                                                        end
                                                    end)

                                                    HealthBar.Color = HealthColor

                                                    HealthBarOutLine.Thickness = Thickness
                                                    HealthBarOutLine.Size = Vector2.new(5, Box.Size.Y)
                                                    
                                                    HealthBarOutLine.Position = Box.Position - Vector2.new(7.4,0)
                                                    HealthBarOutLine.Color = Color3.fromRGB(0, 0, 0)
                                                else
                                                    NameText.Visible = false
                                                    HealthBarOutLine.Visible = false
                                                    HealthBar.Visible = false
                                                    Box.Visible = false
                                                    BoxLine.Visible = false
                                                end
                                            else
                                                NameText.Visible = false
                                                HealthBarOutLine.Visible = false
                                                HealthBar.Visible = false
                                                Box.Visible = false
                                                BoxLine.Visible = false
                                            end
                                        end
                                    end)
                                    task.spawn(function()
                                        game.Players.ChildRemoved:Connect(function(plr)
                                            if plr == v then
                                                REPETICAO:Disconnect()
                                                BoxLine:Remove()
                                                Box:Remove()
                                                HealthBarOutLine:Remove()
                                                HealthBar:Remove()
                                                NameText:Remove()
                                            end
                                        end)
                                    end)
                                    Repeticoes[#Repeticoes + 1] = REPETICAO
                                end

                                coroutine.wrap(ms)()
                            end
                        end
                        for i,v in pairs(game.Players:GetChildren()) do
                            Draw_Box(v)
                        end
                        game.Players.ChildAdded:Connect(function(plr)
                            plr.CharacterAdded:wait()
                            wait(5)
                            Draw_Box(plr)
                        end)
                    end

                    function add_lines()
                        function Draw_line(v)
                            if v ~= speaker then
                                local Line = Drawing.new('Line')
                                Line.Visible = false

                                Esps_Online[#Esps_Online + 1] = Line
                                
                                function mss()
                                    local REPETICAO = game.RunService.RenderStepped:Connect(function()
                                        if Line then
                                            local tv, OnScreen = workspace.CurrentCamera:worldToViewportPoint(getTorso(v).Position - Vector3.new(0, -1, 0))
                                            
                                            if OnScreen then
                                                if EspTeamCheck == true then
                                                    if v.TeamColor ~= speaker.TeamColor and getHumanoid(v).Health > 0 then
                                                        Line.Visible = LineEnabled
                                                    else
                                                        Line.Visible = false
                                                    end
                                                else
                                                    if getHumanoid(v).Health > 0 then
                                                        Line.Visible = LineEnabled
                                                    else
                                                        Line.Visible = false
                                                    end
                                                end
                                                if EspColorTeamColor == true then
                                                    Line.Color = v.TeamColor.Color
                                                else
                                                    Line.Color = getgenv().Color
                                                end
                                                Line.Thickness = Thickness
                                                Line.From = Vector2.new(workspace.CurrentCamera.ViewportSize.X / 2, workspace.CurrentCamera.ViewportSize.Y / 1)
                                                Line.To = Vector2.new(tv.X, tv.Y)
                                            else
                                                Line.Visible = false
                                            end
                                        end
                                    end)
                                    game.Players.ChildRemoved:Connect(function(plr)
                                        if plr == v then
                                            Line:Remove()
                                            REPETICAO:Disconnect()
                                        end
                                    end)
                                    Repeticoes[#Repeticoes + 1] = REPETICAO
                                end

                                coroutine.wrap(mss)()
                            end
                        end

                        for i,v in pairs(game.Players:GetChildren()) do
                            Draw_line(v)
                        end
                        game.Players.ChildAdded:Connect(function(plr)
                            Draw_line(plr)
                        end)
                    end
                    TabEsp:CreateTextLabel('Execute Esps')

                    TabEsp:CreateButton('Run Skeleton.lua', function()
                        add_skeleton()
                        Notify('Skeleton Esp', 'Sucess Run Skeletion Esp', nil, 2)
                    end)
                    TabEsp:CreateButton('Run box.lua', function()
                        add_box()
                        Notify('Box Esp', 'Sucess Run Box Esp', nil, 2)
                    end)
                    TabEsp:CreateButton('Run line.lua', function()
                        add_lines()
                        Notify('Line Esp', 'Sucess Run Line Esp', nil, 2)
                    end)

                    TabEsp:CreateButton('Delete all esps', function()
                        pcall(function()
                            for i = 1,#Repeticoes do
                                Repeticoes[i]:Disconnect()
                            end
                        end)
                        for i = 1,#Esps_Online do
                            pcall(function()
                                if typeof(Esps_Online[i]) == 'table' then
                                    Esps_Online[i]:Remove()
                                elseif typeof(Esps_Online[i]) == 'Instance' then
                                    Esps_Online[i]:Destroy()
                                end
                            end)
                        end
                    end, Color3.fromRGB(230, 0, 0));

                    TabEsp:CreateToggle('Skeleton', function(bool)
                        SkeletionEsp = bool
                    end)
                    TabEsp:CreateToggle('Lines', function(bool)
                        LineEnabled = bool
                    end)
                    TabEsp:CreateToggle('Names', function(bool)
                        NameEnabled = bool
                    end)

                    TabEsp:CreateToggle('Box', function(bool)
                        boxEnabled = bool
                    end)

                    TabEsp:CreateToggle('Health', function(bool)
                        HealthEnabled = bool
                    end)
                    
                    TabEsp:CreateToggle('ESP Color TeamColor', function(b)
                        EspColorTeamColor = b
                    end)

                    TabEsp:CreateToggle('Team check', function(bool)
                        EspTeamCheck = bool
                    end)

                    TabEsp:CreateToggle('Box Fill', function(bool)
                        boxFilled = bool
                    end)

                    TabEsp:CreateToggle('Box outline', function(bool)
                        BoxOutline = bool
                    end)

                    

                    TabEsp:CreateToggle('Text Outline', function(bool)
                        TextOutline = bool
                    end)
                    

                    TabEsp:CreateSlider('Thickness', 0, 5, function(value)
                        Thickness = value
                    end, true)
                    
                    TabEsp:CreateSlider('Box outline Thickness', 0, 5, function(bool)
                        OutLineValue = bool
                    end, true)

                    TabEsp:CreateSlider('Name Font', 1, 3, function(b)
                        TextFont = b
                    end)



                    getgenv().R = 203
                    getgenv().G = 141
                    getgenv().B = 203
                    getgenv().Color = Color3.fromRGB(getgenv().R, getgenv().G, getgenv().B)

                    
                    TabEsp:CreateTextBox('RGB CUSTOM', 'String',function(text)
                        text = string.gsub(text, ' ', '')
                        text = text:split(',')
                        print('Color3.fromRGB('..text[1]..', '..text[2]..', '..text[3]..')')
                        getgenv().R = tonumber(text[1])
                        getgenv().G = tonumber(text[2])
                        getgenv().B = tonumber(text[3])
                        getgenv().Color = Color3.fromRGB(getgenv().R, getgenv().G, getgenv().B)
                    end)
                    
                    TabEsp:CreateSlider('RGB COLOR - R', 0, 255, function(color)
                        getgenv().Color = Color3.fromRGB(getgenv().R, getgenv().G, getgenv().B)
                        getgenv().R = color
                    end)
                    
                    TabEsp:CreateSlider('RGB COLOR - G', 0, 255, function(color)
                        getgenv().Color = Color3.fromRGB(getgenv().R, getgenv().G, getgenv().B)
                        getgenv().G = color
                    end)
                    
                    TabEsp:CreateSlider('RGB COLOR - B', 0, 255, function(color)
                        getgenv().Color = Color3.fromRGB(getgenv().R, getgenv().G, getgenv().B)
                        getgenv().B = color
                    end)

                    TabEsp:CreateWhile('Rainbow Esp', function(bool)
                        for i = 0,255,10 do
                            wait()
                            getgenv().Color = Color3.new(255/255,i/255,0/255)
                        end
                        for i = 255,0,-10 do
                            wait()
                            getgenv().Color = Color3.new(i/255,255/255,0/255)
                        end
                        for i = 0,255,10 do
                            wait()
                            getgenv().Color = Color3.new(0/255,255/255,i/255)
                        end
                        for i = 255,0,-10 do
                            wait()
                            getgenv().Color = Color3.new(0/255,i/255,255/255)
                        end
                        for i = 0,255,10 do
                            wait()
                            getgenv().Color = Color3.new(i/255,0/255,255/255)
                        end
                        for i = 255,0,-10 do
                            wait()
                            getgenv().Color = Color3.new(255/255,0/255,i/255)
                        end
                    end)
                    
                    speedBoost = 10
                    bindBoost = Enum.KeyCode.RightControl
                    Vehicle:CreateSlider('Vehicle Speed', 1, 10, function(value)
                        speedBoost = value
                    end, true)
                    
                    
                    
                    
                    alreadyBind = false
                    boostOn = false
                    Vehicle:CreateToggle('Boost ON', function(bool)
                        boostOn = bool
                    end)
                    Vehicle:CreateSlider('Vehicle gravity', 0, 196.2, function(value)
                        workspace.Gravity = value
                    end)
                    Vehicle:CreateButton('Bind key - Speed boost', function()
                        game.StarterGui:SetCore('SendNotification', {
                            Title = 'Tutorial';
                            Text = 'Press any key to set to Bind key';
                        })
                        local bind = game:GetService('UserInputService').InputBegan:Connect(function(input)
                            bindBoost = input.KeyCode
                            alreadyBind = true
                            game.StarterGui:SetCore('SendNotification', {
                                Title = 'Sucess set bind';
                                Text = 'Your new bind are ';
                            })
                        end)
                        repeat wait() until alreadyBind == true
                        alreadyBind = false
                        bind:Disconnect()
                    end)
                    running = false
                    game:GetService('UserInputService').InputBegan:Connect(function(input)
                        if input.KeyCode == bindBoost and boostOn then
                            running = true;
                            local Character = game.Players.LocalPlayer.Character; 
                            local Humanoid = Character:FindFirstChildWhichIsA("Humanoid"); 
                            local VehicleSeat = Humanoid.SeatPart; 
                            local Vehicle = VehicleSeat:FindFirstAncestorWhichIsA("Model"); 
                            if table.find({"wheels", "body", "misc", "core"}, Vehicle.Name) then 
                                Vehicle = Vehicle.Parent; 
                            end; 
                            local TweenService = game:GetService('TweenService')
                            for index, value in pairs(Vehicle:GetDescendants()) do
                                if value:IsA("BasePart") then 
                                    value.Velocity = value.Velocity + Vector3.new(-speedBoost, 0, -speedBoost)
                                end; 
                            end;
                            task.wait(.5)
                        end                
                    end)
                    game:GetService('UserInputService').InputEnded:Connect(function(input)
                        if input.KeyCode == bindBoost then
                            running = false;
                        end
                        
                    end)
                    
                    
                    Vehicle:CreateButton('Vehicle Fly {H}', function()
                        pcall(function()
                            FLYSPEED = 50
                            repeat wait() until game.Players.LocalPlayer and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:findFirstChild("Head") and game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Humanoid") local mouse = game.Players.LocalPlayer:GetMouse() repeat wait() until mouse local plr = game.Players.LocalPlayer local torso = plr.Character.Head local flying = false local deb = true local ctrl = {f = 0, b = 0, l = 0, r = 0} local lastctrl = {f = 0, b = 0, l = 0, r = 0} local maxspeed = 5000 local speed = 5000 function Fly() local bg = Instance.new("BodyGyro", torso) bg.P = 10000 bg.maxTorque = Vector3.new(9e9, 9e9, 9e9) bg.cframe = torso.CFrame local bv = Instance.new("BodyVelocity", torso) bv.velocity = Vector3.new(0,0.1,0) bv.maxForce = Vector3.new(9e9, 9e9, 9e9) repeat wait() plr.Character:FindFirstChildWhichIsA('Humanoid').PlatformStand = false if ctrl.l + ctrl.r ~= 100000 or ctrl.f + ctrl.b ~= 10000 then speed = speed+.0+(speed/maxspeed) if speed > maxspeed then speed = maxspeed end elseif not (ctrl.l + ctrl.r ~= 5 or ctrl.f + ctrl.b ~= 5) and speed ~= 5 then speed = speed-5 if speed > 5 then speed = -2 end end if (ctrl.l + ctrl.r) ~= 5 or (ctrl.f + ctrl.b) ~= 5 then bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (ctrl.f+ctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(ctrl.l+ctrl.r,(ctrl.f+ctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed lastctrl = {f = ctrl.f, b = ctrl.b, l = ctrl.l, r = ctrl.r} elseif (ctrl.l + ctrl.r) == 5 and (ctrl.f + ctrl.b) == 5 and speed ~= 5 then bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (lastctrl.f+lastctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(lastctrl.l+lastctrl.r,(lastctrl.f+lastctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed else bv.velocity = Vector3.new(0,0.1,0) end bg.cframe = game.Workspace.CurrentCamera.CoordinateFrame * CFrame.Angles(-math.rad((ctrl.f+ctrl.b)*50*speed/maxspeed),0,0) until not flying ctrl = {f = 0, b = 0, l = 0, r = 0} lastctrl = {f = 0, b = 0, l = 0, r = 0} speed = 5 bg:Destroy() bv:Destroy() plr.Character:FindFirstChildWhichIsA('Humanoid').PlatformStand = false end mouse.KeyDown:connect(function(key) if key:lower() == "h" then if flying then flying = false else flying = true Fly() end elseif key:lower() == "w" then ctrl.f = FLYSPEED elseif key:lower() == "s" then ctrl.b = -FLYSPEED elseif key:lower() == "a" then ctrl.l = -FLYSPEED elseif key:lower() == "d" then ctrl.r = FLYSPEED end end) mouse.KeyUp:connect(function(key) if key:lower() == "w" then ctrl.f = 0 elseif key:lower() == "s" then ctrl.b = 0 elseif key:lower() == "a" then ctrl.l = 0 elseif key:lower() == "d" then ctrl.r = 0 end end) Fly()    
                        end);
                    end)
                    
                    Vehicle:CreateButton('Force stop vehicle', function()
                        local Character = game.Players.LocalPlayer.Character; 
                        local Humanoid = Character:FindFirstChildWhichIsA("Humanoid"); 
                        local VehicleSeat = Humanoid.SeatPart; 
                        local Vehicle = VehicleSeat:FindFirstAncestorWhichIsA("Model"); 
